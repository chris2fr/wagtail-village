INSERT INTO public.sites_faciles_contentpage VALUES (115, '[{"id": "fca1608c-0d87-4f34-8843-ce76f8d20bfe", "type": "paragraph", "value": "<h2 data-block-key=\"sxybs\">Conclusions</h2><p data-block-key=\"d24e0\">Les votes connexes aux conclusions sont toujours  venir.</p><ol><li data-block-key=\"9jn2p\">Nous actons la dmission de Mal et donne pouvoir  Chris MANN pour faire les dmarches connexes</li><li data-block-key=\"4j4ig\">Un prvisionnel est  tablir avant d''engager la commande de 3000 euros auprs d''Osorus (Popup Expos)</li><li data-block-key=\"4jqe5\">Une dette de 1000 euros au Bureau du Crateur est  accorder en priorit avant l''engagement financier pour Osorus (Popup Expos)</li><li data-block-key=\"8pfl3\">Le Conseil prend note de la plainte de Chris MANN pour l''appel  SImon LAISNEY, Prsident de la SCIC Plateau Urbain, en ligne avec les actions du collectif Aurore, Plateau-Urbain et Yes We Camp depuis 2017</li><li data-block-key=\"91fla\">Les modifications statutaires consquentes  la dclaration d''intrt gnral sont  prsenter prochainement.</li><li data-block-key=\"b85bf\">Il y a du pain sur la planche</li></ol><h2 data-block-key=\"30k1q\">Droulement</h2><p data-block-key=\"5iqvn\">Le 19e Conseil des voisins s''est droul le 7 aot 2023 en deux temps selon les contraintes des participants: d''abord  11h puis aprs  19h15.</p><h2 data-block-key=\"27s0n\">Sujets traits</h2><ol><li data-block-key=\"9v14k\">Intimidation par la gestionnaire des Arches citoyennes le 4 aot</li><li data-block-key=\"677hl\">Discussion sur les activits  continuer et  dmarrer</li><li data-block-key=\"dggl0\">Les arts voisins</li><li data-block-key=\"b64a0\">Les finances</li><li data-block-key=\"fb6dt\">D''autres sujets divers</li></ol><h2 data-block-key=\"713us\">Invitation et Compte-rendu</h2><p data-block-key=\"2f4dm\"><a href=\"https://www.lesgrandsvoisins.com/structure/conseils-des-voisins/19e-conseil/invitation/?ref=blog.lesgrandsvoisins.com\">Invitation au 19e Conseil des Voisins</a></p><p data-block-key=\"1jlg3\"><a href=\"https://www.lesgrandsvoisins.com/structure/conseils-des-voisins/19e-conseil/compte-rendu/?ref=blog.lesgrandsvoisins.com\">Compte-rendu du 19e conseil des voisins</a></p><p data-block-key=\"e9qsb\"></p><embed embedtype=\"image\" format=\"fullwidth\" id=\"24\" alt=\"\"/><p data-block-key=\"7dfgd\"></p>"}]', ,, 23, 'blue-cumulus', NULL, NULL, true, true, true, NULL));
