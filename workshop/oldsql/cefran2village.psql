INSERT INTO public.wagtailcore_revision VALUES (31, '2024-05-05 17:25:02.502798+00', '{"pk": 14, "body": "[]", "date": "2023-09-20T11:00:00Z", "live": false, "path": "0001000200030008", "slug": "le-numerique-dans-les-assocs", "depth": 4, "owner": 1, "title": "Le numrique dans les assoc''s ?", "locale": 1, "locked": false, "authors": [1], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/le-numerique-dans-les-assocs/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "Le numrique dans les assoc''s ?", "entry_tags": [{"pk": 17, "tag": 22, "content_object": 14}, {"pk": 18, "tag": 15, "content_object": 14}], "go_live_at": null, "draft_title": "Le numrique dans les assoc''s ?", "content_type": 71, "header_image": 22, "header_large": true, "header_darken": true, "live_revision": null, "show_in_menus": true, "blog_categories": [2], "header_cta_link": null, "header_cta_text": "Rencontre du numrique dans les associations le 18 septembre 2023", "latest_revision": null, "translation_key": "6efc121c-f946-4877-9003-c09084512b41", "entry_categories": [{"pk": 10, "page": 14, "category": 2}], "header_cta_label": null, "header_with_title": true, "last_published_at": null, "first_published_at": null, "header_color_class": "yellow-tournesol", "search_description": "Quelle place le numrique dans les associations ? Comment adresser le GDPR (et est-ce que a existe rellement) ? Pourquoi les listes de diffusion sont si chres ? Quel cot le gratuit ? le libre ? Sauvetage ? Wordpress ou ?  Intelligence  artificielle ?", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": null}', NULL, '14', 1, 71, 1, 'Le numrique dans les assoc''s ?');
INSERT INTO public.wagtailcore_revision VALUES (34, '2024-05-05 17:35:46.070364+00', '{"pk": 16, "body": "[{\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"cwh4c\\\">La journ\\u00e9e du 10 mai 2023, l''INCLUSIV''DAY s''est d\\u00e9roul\\u00e9e \\u00e0 la D\\u00e9fense, el Manhattan de PARIS avec la pr\\u00e9sence de deux ministres du government Macron. J''y avais un stand pour LesGrandsVoisins.com.</p><p data-block-key=\\\"4t7v4\\\">La France a enfin reconnu mon c\\u00f4t\\u00e9 \\u00ab Mejedhoob \\u00bb RQTH ayant moi-m\\u00eame un pet au casque qui risque de d\\u00e9gainer sur la politique ext\\u00e9rieure de mon paye si mes droits, en tant qu''ambassadeur auto-proclam\\u00e9 de l''art contempoain de Mauritanie en occident n''est pas reconnu et respect\\u00e9 par l''Etat Mauritanien.</p><p data-block-key=\\\"d990l\\\">Ma rencontre ubuesque avec les deux ministres d\\u00e9note de la diplomatie parall\\u00e8le active que je m\\u00e8ne, en solo, depuis des d\\u00e9cennies sans le moindre \\\"encouragement\\\" des hautes sph\\u00e8res de mon pays, juste parce-que je suis un homme libre des milieux nantis et bandits: \\\"Allah Yenal Sow\\\" !</p><p data-block-key=\\\"25uub\\\">maelanc.com</p><p data-block-key=\\\"23s6t\\\">Ma\\u00ebl ANC</p>\", \"id\": \"e902d99c-9a08-4345-a5fa-365c350f26f2\"}]", "date": "2023-07-04T10:00:00Z", "live": true, "path": "000100020003000A", "slug": "post-it-suite-a-inclusivday-2023", "depth": 4, "owner": 1, "title": "Post It suite  Inclusiv''Day 2023", "locale": 1, "locked": false, "authors": [2], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/post-it-suite-a-inclusivday-2023/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "Post It suite  Inclusiv''Day 2023", "entry_tags": [{"pk": 23, "tag": 23, "content_object": 16}, {"pk": 24, "tag": 15, "content_object": 16}], "go_live_at": null, "draft_title": "Post It suite  Inclusiv''Day 2023", "content_type": 71, "header_image": 25, "header_large": true, "header_darken": true, "live_revision": null, "show_in_menus": true, "blog_categories": [3], "header_cta_link": null, "header_cta_text": null, "latest_revision": null, "translation_key": "ee7cb000-4008-44b0-b58c-382642f9386c", "entry_categories": [{"pk": 12, "page": 16, "category": 3}], "header_cta_label": null, "header_with_title": true, "last_published_at": null, "first_published_at": null, "header_color_class": "yellow-tournesol", "search_description": "La journe du 10 mai 2023, l''INCLUSIV''DAY s''est droule  la Dfense, el Manhattan de PARIS avec la prsence de deux ministres du government Macron. J''y avais un stand pour LesGrandsVoisins.com.", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": null}', NULL, '16', 1, 71, 1, 'Post It suite  Inclusiv''Day 2023');
INSERT INTO public.wagtailcore_revision VALUES (162, '2024-05-24 14:55:42.122625+00', '{"pk": 36, "body": "[{\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"kokwt\\\">Aux Grands Voisins nous avons <a linktype=\\\"page\\\" id=\\\"30\\\">un manifeste</a> \\u00e9dit\\u00e9e par Chris Mann avec des contributions de plus de cent personnes sur deux grands ateliers et plusieurs s\\u00e9ances de travail sur une p\\u00e9riode d''un mois en 2017. Chacun a un avis qui est le sien sur ce que c''est \\u00eatre Grands Voisins et chacun a raison.</p><p data-block-key=\\\"b4dti\\\">L''objectif des activit\\u00e9s dans notre cellule civisme est de promouvoir ce qu''il faut pour avoir les possibilit\\u00e9s de rencontres ou non. Cela d\\u00e9coule m\\u00eame de l''all\\u00e9gorie du voisin ; l''on peut rester chez soi, l''on peut sortir, l''on peut inviter d''autres.</p><p data-block-key=\\\"aovvo\\\">Concr\\u00e8tement, nous pouvons commencer par un annuaire QuiQuoiEtc qui permet de r\\u00e9f\\u00e9rencer les voisins qui veulent bien. Cela permet une communititude et des possibilit\\u00e9s de compr\\u00e9hensions, empathies et entraides int\\u00e9ressantes.</p><p data-block-key=\\\"4j94g\\\"></p>\", \"id\": \"e569e14e-92d4-476c-96b3-2b221f2ce890\"}]", "live": true, "path": "000100020007", "slug": "civisme", "depth": 3, "owner": 1, "title": "Civisme", "locale": 1, "locked": false, "expired": false, "alias_of": null, "numchild": 1, "url_path": "/home/civisme/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "", "go_live_at": null, "draft_title": "Civisme", "content_type": 25, "header_image": null, "header_large": false, "header_darken": false, "live_revision": 106, "show_in_menus": true, "header_cta_link": null, "header_cta_text": null, "latest_revision": 106, "translation_key": "8a173765-a6a8-40e1-89ea-de7f1986d532", "contentpage_tags": [], "header_cta_label": null, "header_with_title": false, "last_published_at": "2024-05-07T14:46:21.539Z", "first_published_at": "2024-05-07T07:28:54.538Z", "header_color_class": null, "search_description": "Aux Grands Voisins nous avons un manifeste dite par Chris Mann avec des contributions de plus de cent personnes sur []", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": "2024-05-07T14:46:21.487Z"}', NULL, '36', 1, 25, 1, 'Civisme');
INSERT INTO public.wagtailcore_revision VALUES (50, '2024-05-05 18:18:01.8654+00', '{"pk": 22, "body": "[{\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"r0ve0\\\">Au sens de l''1.b, et accessoirement aussi de l''1.e et de l''1.f bis, de l''article 200 (dons des particuliers), et \\u00e0 l''1.a de l''article 238 bis (dons des entreprises) du code g\\u00e9n\\u00e9ral des imp\\u00f4ts, et selon la jurisprudence de la Cour de cassation du 9 f\\u00e9vrier 2017 (2e chambre civile 16-12423 et 16-11483), \\u00a0l''association coop\\u00e9rative Les Grands Voisins est un organisme d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral en capacit\\u00e9 de d\\u00e9livrer des certificats Cerfa n. 11580*03 qui permet aux individus de r\\u00e9duire leurs imp\\u00f4ts de 66% du montant de leur don et aux entreprises de r\\u00e9duire de 60% de leur don. Notre qualification est \\u00e9tablie par vote du 17e Conseil des voisins le 2 mai 2023 par la nature de nos activit\\u00e9s et nos modes d''excercice ainsi justifi\\u00e9es ci-apr\\u00e8s.</p><h2 data-block-key=\\\"ae6bj\\\">Notre conformit\\u00e9 aux crit\\u00e8res d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral</h2><p data-block-key=\\\"s5m9\\\">Trois crit\\u00e8res sont requis pour les dons des particuliers (<a href=\\\"https://bofip.impots.gouv.fr/bofip/5825-PGP.html/identifiant=BOI-IR-RICI-250-10-10-20170510?ref=blog.lesgrandsvoisins.com\\\">BOI-IR-RICI-250-10-10-20170510</a> 90 - 100) :</p><p data-block-key=\\\"8jgs1\\\">Un organisme est d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral au sens du b du 1 de l''<a href=\\\"https://www.legifrance.gouv.fr/codes/article_lc/LEGIARTI000034597042/2017-05-05?ref=blog.lesgrandsvoisins.com\\\">article 200 du CGI</a> d\\u00e8s lors qu''il remplit cumulativement les trois conditions suivantes :<br/>- il n''exerce pas d''activit\\u00e9 lucrative au sens du 1 de l''<a href=\\\"https://www.legifrance.gouv.fr/codes/article_lc/LEGIARTI000034597017/2017-05-05?ref=blog.lesgrandsvoisins.com\\\">article 206 du CGI</a> (<a href=\\\"https://bofip.impots.gouv.fr/doctrine/pgp/2343-PGP?ref=blog.lesgrandsvoisins.com\\\">BOI-IS-CHAMP-10-50-10</a>) ;<br/>- il ne fait pas l''objet d''une gestion int\\u00e9ress\\u00e9e ;<br/>- il ne fonctionne pas au profit d''un cercle restreint de personnes.<br/>Si la reconnaissance d''utilit\\u00e9 publique constitue un indice du respect de ces trois conditions, les organismes qui b\\u00e9n\\u00e9ficient de cette reconnaissance doivent n\\u00e9anmoins les satisfaire effectivement.</p><p data-block-key=\\\"2s16t\\\">et par des personnes morales (<a href=\\\"https://bofip.impots.gouv.fr/bofip/10963-PGP.html/identifiant=BOI-BIC-RICI-20-30-10-10-20220608?ref=blog.lesgrandsvoisins.com#OEuvres_ou_organismes_dinte_20\\\">BOI-BIC-RICI-20-30-10-10-20220608</a> 80 - 170)</p><p data-block-key=\\\"4bhpv\\\">L''organisme b\\u00e9n\\u00e9ficiaire des dons et versements doit \\u00eatre d\\u2019int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral. Cette condition est remplie si l''activit\\u00e9 n''est pas lucrative, si la gestion est d\\u00e9sint\\u00e9ress\\u00e9e et si l\\u2019organisme ne fonctionne pas au profit d\\u2019un cercle restreint de personnes.<br/>[...]</p><p data-block-key=\\\"c28gf\\\">Ce sont respectivement les notes d''application respectivement pour les articles 200 et 238 bis du code g\\u00e9n\\u00e9ral des imp\\u00f4ts. La d\\u00e9finition de l''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral pour les dons des individus est produite en int\\u00e9gralit\\u00e9. La d\\u00e9finition pour les dons des entreprises comporte neuf paragraphes en plus divis\\u00e9s en les trois parties : activit\\u00e9 non-lucrative, gestion d\\u00e9sint\\u00e9ress\\u00e9e et organismes ne fonctionnant pas au profit d''un cercle restraint de personnes.</p><p data-block-key=\\\"4hq7f\\\">Le Conseil des voisins du 2 mai 2023 prend acte de la totalit\\u00e9 des exigences et nous d\\u00e9clare en conformit\\u00e9 avec l''ensemble des exigences. Au moment de l''\\u00e9criture de cet article, le 29 avril 2023, ce vote n''a pas encore eu lieu.</p><p data-block-key=\\\"8frri\\\">Il convient aussi d''\\u00e9claircir comment nous sommes en conformit\\u00e9 \\u00e0 la notion fiscale d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral. De toute les fa\\u00e7ons, notre qualification peut \\u00eatre contest\\u00e9 devant un juge, et nous en aurions \\u00e0 nous justifier pour continuer \\u00e0 toujours \\u00e9mettre des re\\u00e7us fiscaux. Quand nous aurions plus de 500.000 euros de dons, nous aurions int\\u00e9r\\u00eat \\u00e0 faire une demande de pr\\u00e9caution aupr\\u00e8s des imp\\u00f4ts.</p><h2 data-block-key=\\\"83iim\\\">Notre conformit\\u00e9 \\u00e0 l''absence d''activit\\u00e9 lucrative</h2><p data-block-key=\\\"9ti3r\\\">Tout d''abord, nous sommes une association fran\\u00e7aise de loi 1901. En cons\\u00e9quence, nous ne distribuons pas de dividendes et nous ne faisons pas de profits.</p><p data-block-key=\\\"ej8kv\\\">Ensuite, notre posture est neutre. Notre force vient de cette neutralit\\u00e9.</p><p data-block-key=\\\"e2jbp\\\">D''autres observations m\\u00e9ritent d''\\u00eatre cit\\u00e9es ici prochainement, mais ce qui pr\\u00e9c\\u00e8de me semble satisfaisant. Dans le cas des dons de personnes morales, il convient de reproduire int\\u00e9gralement l''instruction de l''article 238 bis et adresser aussi nos observations, bien que ce qui pr\\u00e9c\\u00e8de donneraient des \\u00e9l\\u00e9ments suffisants.</p><h2 data-block-key=\\\"c9g7l\\\">Notre conformit\\u00e9 \\u00e0 la gestion d\\u00e9sint\\u00e9ress\\u00e9e</h2><p data-block-key=\\\"c8a73\\\">Encore, notre force est notre neutralit\\u00e9 et nous sommes une association fran\\u00e7aise de loi 1901. La remuneration du pr\\u00e9sident est limit\\u00e9 \\u00e0 un seuil inf\\u00e9rieur au SMIC.</p><p data-block-key=\\\"for0p\\\">La question de d\\u00e9sint\\u00e9ressement est profonde et va au-d\\u00e9l\\u00e0 de la gestion de l''association, mais nous allons traiter ici le mode de gestion.</p><p data-block-key=\\\"4q8ki\\\">Tout d''abord, qui est Grand Voisin qui veut bien l''\\u00eatre. Nous avons trois coll\\u00e8ges avec des poids \\u00e9gaux de vote. Chaque personne \\u00e0 l''int\\u00e9rieur de chaque coll\\u00e8ge \\u00e0 un poids \\u00e9gal. C''est notre mani\\u00e8re de prendre nos d\\u00e9cisions.</p><p data-block-key=\\\"hda7\\\">Tout acte de gestion \\u00e9mane obligatoirement d''un vote dans un Conseil des Voisins. En cons\\u00e9quence, la gestion est d\\u00e9sint\\u00e9ress\\u00e9e.</p><p data-block-key=\\\"fsrjb\\\">Il est \\u00e9galement important de noter que notre fonctionnement est reconnu en conformit\\u00e9 par les f\\u00e9d\\u00e9rations internationaux des Coop\\u00e9rative, chose qui garantie une neutralit\\u00e9 en plus dans notre gestion. Cette neutralit\\u00e9, en combination avec notre ouverture totale (voir ci-dessous) rend la gestion compl\\u00e8tement d\\u00e9sint\\u00e9ress\\u00e9e.</p><h2 data-block-key=\\\"bn47\\\">Le b\\u00e9n\\u00e9fice \\u00e0 un cercle ouvert</h2><p data-block-key=\\\"55gkv\\\">Tout d''abord, la d\\u00e9cision ou non d''\\u00eatre membre des Grands Voisins, et donc de disposer d''une voix et de demander des comptes, d\\u00e9pend de la personne. L''association coop\\u00e9rative n''a rien \\u00e0 dire r\\u00e9lative \\u00e0 la d\\u00e9cision de la personne.</p><p data-block-key=\\\"8hjll\\\">Notre manifeste d\\u00e9clare \\\"L''inclusion et l''exclusion, toutes les deux, peuvent nous \\u00e9loigner de l''autre qui est une chance.\\\" Notre d\\u00e9finition m\\u00eame est la notion de Voisins. Par d\\u00e9finition, on est toutes et tous voisins !</p>\", \"id\": \"cebf7f07-4f2c-4787-abfd-946835a59e76\"}, {\"type\": \"image\", \"value\": {\"title\": \"\", \"heading_tag\": \"h3\", \"image\": 37, \"alt\": \"\", \"caption\": \"\", \"url\": \"\"}, \"id\": \"c5a0580a-9ff9-46dd-9a03-1b3d3dc30de0\"}]", "date": "2023-04-29T10:00:00Z", "live": true, "path": "000100020003000G", "slug": "interet-general", "depth": 4, "owner": 1, "title": "Intrt gnral", "locale": 1, "locked": false, "authors": [1], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/interet-general/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "Intrt gnral", "entry_tags": [{"pk": 39, "tag": 10, "content_object": 22}, {"pk": 40, "tag": 15, "content_object": 22}], "go_live_at": null, "draft_title": "Intrt gnral", "content_type": 71, "header_image": 37, "header_large": true, "header_darken": true, "live_revision": null, "show_in_menus": true, "blog_categories": [3], "header_cta_link": null, "header_cta_text": null, "latest_revision": null, "translation_key": "fa822ad1-a035-4fd8-a0a8-450aa52b72f3", "entry_categories": [{"pk": 18, "page": 22, "category": 3}], "header_cta_label": null, "header_with_title": true, "last_published_at": null, "first_published_at": null, "header_color_class": "pink-macaron", "search_description": "Article en cours d''laboration. Nous sommes un organisme d''intrt gnral habilit  dlivrer des reus fiscaux aux donateurs individus (art. 200 CGI) et aux organisations (art. 238 bis) permettant des rductions d''impt de 66% et 60% respectivement de vos dons.", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": null}', NULL, '22', 1, 71, 1, 'Intrt gnral');
INSERT INTO public.wagtailcore_revision VALUES (56, '2024-05-05 18:47:04.831127+00', '{"pk": 24, "body": "[{\"type\": \"quote\", \"value\": {\"image\": 40, \"quote\": \"Qui est Grands Voisin qui veut bien l''\\u00eatre. Cette Marque b\\u00e9n\\u00e9ficie d''un double-d\\u00e9po\\u00f4t qui ouvre effectivement sa propri\\u00e9t\\u00e9 \\u00e0 toutes et \\u00e0 tous.\", \"author_name\": \"Les Grands Voisins, une marque pour toutes et pour tous\", \"author_title\": \"\", \"color\": \"\"}, \"id\": \"beef652d-7693-4010-b4c4-c5e1b9119295\"}, {\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"rpyh7\\\">J''ai \\u00e9t\\u00e9 attaqu\\u00e9 pour plusieurs raisons. Souvent, on m''accuse d''instiguer les agresseuses et les agresseurs. \\u00ab Pour qui me prendrai-je ? \\u00bb l''un peu penser. J''ai pass\\u00e9 un bon moment \\u00e0 r\\u00e9fl\\u00e9chir \\u00e0 cette proposition sous formes diff\\u00e9rentes.</p><p data-block-key=\\\"37q43\\\">Au contraire des \\u00e9tiquettes r\\u00e9currentes que je re\\u00e7ois, peut-\\u00eatre m\\u00eame de mani\\u00e8re coordonn\\u00e9e, augment\\u00e9e et accentu\\u00e9e, je pense que mes actions personnelles sont mieux guid\\u00e9es en me consid\\u00e9rant comme \\u00ab n''importe qui \\u00bb. Par hasard, je me retrouve personnellement avec la responsabilit\\u00e9 de la Marque Les Grands Voisins en m\\u00eame temps qu''un d\\u00e9p\\u00f4t collectif de trois organisations puissantes que je nommerai ci-apr\\u00e8s \\u00ab le Consortium \\u00bb. \\u00a0Ce court article permettrait de poser rapidement des observations partiales face \\u00e0 des accusations tr\\u00e8s moches et destructrices contre moi.</p><h2 data-block-key=\\\"7fpqg\\\">Je suis Grand Voisin</h2><p data-block-key=\\\"1vv8q\\\">N''importe qui peut \\u00eatre Grand Voisin. Je suis n''importe qui. Donc, je peux \\u00eatre Grand Voisin. C''est une propri\\u00e9t\\u00e9 transitive. De m\\u00eame, si vous pouvez admettre aussi v\\u00f4tre c\\u00f4t\\u00e9 \\u00ab n''importe qui \\u00bb, en plus de tout d''\\u00eatre particulier et unique, vous aussi, vous pouvez \\u00eatre Grand Voisin.</p><p data-block-key=\\\"2p8h0\\\">Il se trouve que j''ai \\u00e9dit\\u00e9 une version d''un document \\u00ab Manifeste \\u00bb des Grands Voisins : un document avec des contributions de plus de cent personnes sur deux grands ateliers sur un mois en 2017. Tout de suite \\u00e0 cette \\u00e9laboration du Manifeste, et dans un contexte de mutation de culture de management du lieu Saint-Vincent-de-Paul, j''ai mont\\u00e9 et ai annonc\\u00e9 sur les voies de communication internes un wiki en fran\\u00e7ais, en arabe et en anglais de d\\u00e9bat sur le site web du nom de domaine de mon d\\u00e9p\\u00f4t wiki.lesgrandsvoisins.com. \\u00a0Sous trois mois, le wiki a \\u00e9t\\u00e9 cyber-attaqu\\u00e9.</p><p data-block-key=\\\"8d3rd\\\">Plus d''une ann\\u00e9e passe. Le 19 f\\u00e9vrier 2019, je me reveille avec une impulsion de d\\u00e9poser la marque \\u00ab Les Grands Voisins \\u00bb. Il n''il y a avait jamais eu de discussion avec qui que ce soit sur le sujet de la marque Les Grands Voisins. Je n''y ai pas pens\\u00e9 la veille, de m\\u00eame le jour apr\\u00e8s avoir d\\u00e9pos\\u00e9 la marque. Trois mois plus tard, je re\\u00e7ois la confirmation de l''INPI et je v\\u00e9rifie si une autre personne ne l''aurait pas d\\u00e9pos\\u00e9e entretemps. Il se trouve que le Consortium a d\\u00e9pos\\u00e9 effectivement la m\\u00eame marque (sauf avec un logo) en collectif le m\\u00eame jour que moi. Un employ\\u00e9 de l''INPI me d\\u00e9clare que dans ces 42 ans \\u00e0 l''INPI, il n''a jamais vu ce cas. Nous avons tous les deux \\u2013 le Consortium et moi-m\\u00eame \\u2013 les m\\u00eames droits dans un r\\u00e9gime de simultan\\u00e9it\\u00e9 de la marque (r\\u00e9gime jusqu''\\u00e0 une r\\u00e9forme de d\\u00e9cembre 2019). Moi et le Consortium sont voisins.</p><p data-block-key=\\\"6aaqf\\\">C''est une bonne chose que nous soyons deux \\u00e0 d\\u00e9tenir la Marque Les Grands Voisins. D''une part, cela rend la marque potentiellement une propri\\u00e9t\\u00e9 ouverte car si un tiers veut s''en servrir, effectivement moi et le Consortium devons effectivement tous deux nous y opposer. C''est parfaitement \\u00e0 l''image des Grands Vosins. D''autre part, pour moi personnellement, je suis amateur de th\\u00e9ologie (par exemple, j''ai c\\u00e9l\\u00e9br\\u00e9 un mariage religieux sur le site Saint-Vincent-de-Paul). L''histoire de ma journ\\u00e9e du 19 f\\u00e9vrier 2019 est romantique et divine. Cette histoire, pour tout \\u00e9tudiant de th\\u00e9ologie, est une histoire belle \\u00e0 cultiver.</p><p data-block-key=\\\"4o49e\\\">La Marque est si forte. Notre h\\u00e9ritage commun est si important. Le monde est meilleur avec Les Grands Voisins. Nous essayons de voir en quelle capacit\\u00e9 cette marque peut \\u00eatre appliqu\\u00e9 au-d\\u00e9l\\u00e0 du lieu contraint qui \\u00e9tait Saint-Vincent-de-Paul. Les autres ne veulent pas utiliser la marque, mais la Marque a sa place de par sa proposition intrins\\u00e8que.</p><p data-block-key=\\\"2ueh4\\\">Il y a une belle juxtaposition d''id\\u00e9es dans la Marque. Tout d''abord, lorsque l''on utilise \\u00ab les \\u00bb, l''on fait r\\u00e9f\\u00e9rence \\u00e0 une d\\u00e9Marquation. On parle des \\u00e9tranger, des autres, des am\\u00e9ricaines, ..., les Autres. Puis, on dit que les Autres sont \\u00ab Grands \\u00bb. C''est d\\u00e9j\\u00e0 une force. Enfin, on parle des autres, car on parle des \\u00ab Voisins \\u00bb, c''est \\u00e0 dire ceux qui sont en dehors de chez soi. La Marque est ing\\u00e9nie. Son manifeste est bien \\u00e0 son image.</p><p data-block-key=\\\"6o7ea\\\">Pour ma part, je suis l\\u00e9gitime uniquement par ma qualit\\u00e9 de n''importe qui. Je suis l\\u00e9gitime car je ne suis pas ill\\u00e9gitime.</p><p data-block-key=\\\"6n6h8\\\">L''id\\u00e9e de l''association coop\\u00e9rative Les Grands Voisins est d''appliquer le concept sans la contrainte des murs.</p><h2 data-block-key=\\\"80r53\\\">Le Civisme serait contagieux</h2><p data-block-key=\\\"5ahj7\\\">Je suis attaqu\\u00e9. Les attaques ont men\\u00e9 des tiers, au moins trois, mais probablement plus. J''ai eu gain de cause dans un proc\\u00e8s civil en 2021, puis certaines actions sont all\\u00e9s loin. Cela fait toujours mal. Ce sera pour une prochaine fois.</p>\", \"id\": \"16e9ae05-58e3-4a2a-b394-7f46a2ec0280\"}]", "date": "2024-05-05T18:41:00Z", "live": true, "path": "000100020003000I", "slug": "la-marque-les-grands-voisins-est-une-responsabilite-pour-nimporte-qui", "depth": 4, "owner": 1, "title": "La Marque Les Grands Voisins est une responsabilit pour n''importe qui", "locale": 1, "locked": false, "authors": [], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/la-marque-les-grands-voisins-est-une-responsabilite-pour-nimporte-qui/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "Qui est Grands Voisin qui veut bien l''tre. Cette Marque bnficie d''un double-dpt qui ouvre effectivement sa proprit  toutes et  tous. [Version 2023-04-29-1]", "entry_tags": [], "go_live_at": null, "draft_title": "La Marque Les Grands Voisins est une responsabilit pour n''importe qui", "content_type": 71, "header_image": 40, "header_large": false, "header_darken": false, "live_revision": 55, "show_in_menus": true, "blog_categories": [], "header_cta_link": null, "header_cta_text": null, "latest_revision": 55, "translation_key": "d061e5f7-c600-48ec-a8e6-5abdb484a1d1", "entry_categories": [], "header_cta_label": null, "header_with_title": true, "last_published_at": "2024-05-05T18:45:47.884Z", "first_published_at": "2024-05-05T18:45:47.884Z", "header_color_class": null, "search_description": "Qui est Grands Voisin qui veut bien l''tre. Cette Marque bnficie d''un double-dpt qui ouvre effectivement sa proprit  toutes et  tous.", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": "2024-05-05T18:45:47.855Z"}', NULL, '24', 1, 71, 1, 'La Marque Les Grands Voisins est une responsabilit pour n''importe qui');
INSERT INTO public.wagtailcore_revision VALUES (95, '2024-05-06 12:18:01.658433+00', '{"pk": 34, "body": "[{\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"yzz1i\\\">La DINUM a eu la gr\\u00e2ce de publier son moteur CMS pour numerique.gouv.fr sur github. Nous nous donnons comme mission de pouvoir rendre le b\\u00e9n\\u00e9fice de ce moteur aux acteurs priv\\u00e9s par le fait de remplacer la charte graphique de l''Etat par une charte g\\u00e9n\\u00e9rique, des chartes g\\u00e9n\\u00e9riques. Voici le lieu de notre travail:</p><p data-block-key=\\\"9fir4\\\"><a href=\\\"https://github.com/chris2fr/sites-faciles\\\">https://github.com/chris2fr/sites-faciles</a></p><p data-block-key=\\\"f6rl6\\\">Les choix prix par le DINUM, \\u00e0 partir du m\\u00eame socle technologique des Grands Voisins -- Wagtail, Django, Python -- sont structurants et rationnels. Le fait d''ouvrir ces d\\u00e9veloppements \\u00e0 un grand nombre nous permet de reverser \\u00e0 l''Etat nos contributions modestes. C''est un exemple de la puissance du dynamique des logiciels libres.</p><p data-block-key=\\\"8la9t\\\">Ce site en travaux est disponible sur <a href=\\\"https://facile.lesgrandsvoisins.com\\\">https://facile.lesgrandsvoisins.com</a>.</p><p data-block-key=\\\"fmm91\\\">Nous avons aussi essay\\u00e9 un site statique sur <a href=\\\"https://les.gv.coop\\\">https://les.gv.coop</a></p><p data-block-key=\\\"3djgc\\\">Le site actuel est sur <a href=\\\"https://www.lesgrandsvoisins.com\\\">https://www.lesgrandsvoisins.com</a></p>\", \"id\": \"87661a48-af93-4913-b933-f0afb4d285b7\"}, {\"type\": \"stepper\", \"value\": {\"title\": \"Mise en place du nouveau site web sous Sites-Faciles\", \"total\": 3, \"current\": 1, \"steps\": [{\"type\": \"step\", \"value\": {\"title\": \"Installation et tests\", \"detail\": \"Il s''agit d''installer le dispositif depuis sites-faciles, de faire des tests et d''ajuster le look-and-feel.\"}, \"id\": \"9a7f37fb-22e6-4878-80cd-8f8e31925334\"}, {\"type\": \"step\", \"value\": {\"title\": \"Validation et contenu\", \"detail\": \"Il s''agit d''organiser l''information et affiner la pr\\u00e9sentation avec une exp\\u00e9rience utilisateur convenable.\"}, \"id\": \"19234db7-2154-489c-9c57-d116199345c8\"}, {\"type\": \"step\", \"value\": {\"title\": \"Installation et mise en production\", \"detail\": \"Il s''agit de faire en sorte que le site web se charge rapidement et est conforme aux besoins de l''association et du Monde.\"}, \"id\": \"48947ec7-5ab0-48a9-8957-d813fc032715\"}]}, \"id\": \"1eea2c01-8d52-4398-8626-9f5298487fc0\"}, {\"type\": \"multicolumns\", \"value\": {\"bg_image\": null, \"bg_color_class\": \"\", \"bg_color\": \"\", \"title\": \"\", \"heading_tag\": \"h2\", \"columns\": [{\"type\": \"text\", \"value\": \"<h3 data-block-key=\\\"l1q4l\\\">Appropriation du dispositif de la DINUM</h3><p data-block-key=\\\"5d2ak\\\">Le dispositif <a href=\\\"https://sites-faciles.beta.numerique.gouv.fr/\\\">sites-faciles</a> de la DINUM est tr\\u00e8s \\u00e9troitement li\\u00e9 aux graphismes de l''Etat, mais a aussi des choix pratiques et communicatives pour des sites webs en France. Nous avons enregistr\\u00e9s <a href=\\\"https://github.com/numerique-gouv/sites-faciles/issues\\\">quelques items</a> avec la DINUM pour travailler sur ces sujets.</p>\", \"id\": \"bb0d1e3a-a505-44bd-ba0b-3663b6ab2b06\"}, {\"type\": \"text\", \"value\": \"<h3 data-block-key=\\\"00gq4\\\">Cr\\u00e9ation des contenus des GV</h3><p data-block-key=\\\"8f2nt\\\">Nous avons des contenus sur <a href=\\\"https://wagtail.lesgrandsvoisins.com\\\">wagtail.resdigita.com</a> et sur <a href=\\\"https://blog.lesgrandsvoisins.com\\\">blog.lesgrandsvoisins.com</a> \\u00e0 migrer sur ce nouveau dispositif.</p>\", \"id\": \"1d3be829-86b6-4bd6-93a8-64a11555df9f\"}]}, \"id\": \"70854f40-8c96-413f-b377-d9a1d1b0435a\"}]", "date": "2024-05-06T08:37:00Z", "live": true, "path": "000100020003000O", "slug": "sites-faciles", "depth": 4, "owner": 1, "title": "Refonte 