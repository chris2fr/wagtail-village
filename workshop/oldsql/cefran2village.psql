INSERT INTO public.wagtailcore_revision VALUES (55, '2024-05-05 18:45:47.855087+00', '{"pk": 24, "body": "[]", "date": "2024-05-05T18:41:00Z", "live": true, "path": "000100020003000I", "slug": "la-marque-les-grands-voisins-est-une-responsabilite-pour-nimporte-qui", "depth": 4, "owner": 1, "title": "La Marque Les Grands Voisins est une responsabilit pour n''importe qui", "locale": 1, "locked": false, "authors": [], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/la-marque-les-grands-voisins-est-une-responsabilite-pour-nimporte-qui/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "Qui est Grands Voisin qui veut bien l''tre. Cette Marque bnficie d''un double-dpt qui ouvre effectivement sa proprit  toutes et  tous. [Version 2023-04-29-1]", "entry_tags": [], "go_live_at": null, "draft_title": "La Marque Les Grands Voisins est une responsabilit pour n''importe qui", "content_type": 71, "header_image": 40, "header_large": false, "header_darken": false, "live_revision": null, "show_in_menus": true, "blog_categories": [], "header_cta_link": null, "header_cta_text": null, "latest_revision": null, "translation_key": "d061e5f7-c600-48ec-a8e6-5abdb484a1d1", "entry_categories": [], "header_cta_label": null, "header_with_title": true, "last_published_at": null, "first_published_at": null, "header_color_class": null, "search_description": "Qui est Grands Voisin qui veut bien l''tre. Cette Marque bnficie d''un double-dpt qui ouvre effectivement sa proprit  toutes et  tous.", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": null}', NULL, '24', 1, 71, 1, 'La Marque Les Grands Voisins est une responsabilit pour n''importe qui');
INSERT INTO public.wagtailcore_revision VALUES (63, '2024-05-06 03:16:26.301705+00', '{"pk": 29, "body": "[{\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"06ccm\\\">Le web est \\u00e9trange. Nous avons essay\\u00e9 les syst\\u00e8mes de contenu suivants :</p><ol><li data-block-key=\\\"bo27f\\\">Wordpress</li><li data-block-key=\\\"so9\\\">Wagtail</li><li data-block-key=\\\"55fhf\\\">ApostropheCMS</li><li data-block-key=\\\"eelv9\\\">HTML</li><li data-block-key=\\\"el2oe\\\">Ghost.io</li><li data-block-key=\\\"4opnk\\\">Plone</li><li data-block-key=\\\"etv36\\\">Odoo</li></ol><p data-block-key=\\\"equoc\\\">Mais finalement, je pense que Ghost peut convenir.</p><p data-block-key=\\\"cj4rn\\\">Il y a beaucoup \\u00e0 ne pas aimer dans ghost.io. C''est un stack technologique tr\\u00e8s limit\\u00e9e: NodeJS et MySQL (version 8) par exemple. Pourtant, il semble avoir une approche native suffisamment orient\\u00e9e pour les besoins de raconter sereinement des histoires pour le web, comme \\u00a0peut le faire Wordpress. Au contraire de Wordpress, nous ne pouvons que faire ce pour quoi est fait Ghost. Notamment, je ne vois pas de mani\\u00e8re de relier les comptes Ghost avec les comptes centralis\\u00e9s des Grands Voisins: chose qui emb\\u00eate pour maintenant. Pourtant, si nous arrivons \\u00e0 \\u00e9crire des choses intelligibles pouvant \\u00eatre lu par des tiers, cela me suffira.</p>\", \"id\": \"ed0e9706-2856-441f-bebc-b79a6643d7eb\"}]", "date": "2024-05-06T03:14:00Z", "live": true, "path": "000100020003000N", "slug": "retour-sur-ghostio-comme-techno-de-site-web", "depth": 4, "owner": 1, "title": "Retour sur Ghost.io comme techno de site web", "locale": 1, "locked": false, "authors": [], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/retour-sur-ghostio-comme-techno-de-site-web/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "Retour sur Ghost.io comme techno de site web", "entry_tags": [], "go_live_at": null, "draft_title": "Retour sur Ghost.io comme techno de site web", "content_type": 71, "header_image": 47, "header_large": true, "header_darken": true, "live_revision": null, "show_in_menus": true, "blog_categories": [], "header_cta_link": null, "header_cta_text": null, "latest_revision": null, "translation_key": "b1b86cd6-853a-420a-a62f-7798d02d794c", "entry_categories": [], "header_cta_label": null, "header_with_title": true, "last_published_at": null, "first_published_at": null, "header_color_class": "pink-tuile", "search_description": "Nous utilisons le logiciel libre Ghost.io pour notre blog.", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": null}', NULL, '29', 1, 71, 1, 'Retour sur Ghost.io comme techno de site web');
INSERT INTO public.wagtailcore_revision VALUES (80, '2024-05-06 08:46:27.23582+00', '{"pk": 34, "body": "[{\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"yzz1i\\\">La DINUM a eu la gr\\u00e2ce de publier son moteur CMS pour numerique.gouv.fr sur github. Nous nous donnons comme mission de pouvoir rendre le b\\u00e9n\\u00e9fice de ce moteur aux acteurs priv\\u00e9s par le fait de remplacer la charte graphique de l''Etat par une charte g\\u00e9n\\u00e9rique, des chartes g\\u00e9n\\u00e9riques. Voici le lieu de notre travail:</p><p data-block-key=\\\"9fir4\\\"><a href=\\\"https://github.com/chris2fr/sites-faciles\\\">https://github.com/chris2fr/sites-faciles</a></p><p data-block-key=\\\"f6rl6\\\">Les choix prix par le DINUM, \\u00e0 partir du m\\u00eame socle technologique des Grands Voisins -- Wagtail, Django, Python -- sont structurants et rationnels. Le fait d''ouvrir ces d\\u00e9veloppements \\u00e0 un grand nombre nous permet de reverser \\u00e0 l''Etat nos contributions modestes. C''est un exemple de la puissance du dynamique des logiciels libres.</p><p data-block-key=\\\"8la9t\\\">Ce site en travaux est disponible sur <a href=\\\"https://facile.lesgrandsvoisins.com\\\">https://facile.lesgrandsvoisins.com</a>.</p><p data-block-key=\\\"fmm91\\\">Nous avons aussi essay\\u00e9 un site statique sur <a href=\\\"https://les.gv.coop\\\">https://les.gv.coop</a></p><p data-block-key=\\\"3djgc\\\">Le site actuel sur</p><p data-block-key=\\\"bafgb\\\"><a href=\\\"https://www.lesgrandsvoisins.com\\\">https://www.lesgrandsvoisins.com</a></p><p data-block-key=\\\"6k49\\\"></p>\", \"id\": \"87661a48-af93-4913-b933-f0afb4d285b7\"}]", "date": "2024-05-06T08:37:00Z", "live": true, "path": "000100020003000O", "slug": "sites-faciles", "depth": 4, "owner": 1, "title": "Refonte web avec Sites Faciles", "locale": 1, "locked": false, "authors": [1], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/refonte-de-pages-avec-sites-faciles/", "expire_at": 