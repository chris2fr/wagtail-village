INSERT INTO public.wagtailcore_revision VALUES (92, '2024-05-06 11:47:13.556759+00', '{"pk": 34, "body": "[{\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"yzz1i\\\">La DINUM a eu la gr\\u00e2ce de publier son moteur CMS pour numerique.gouv.fr sur github. Nous nous donnons comme mission de pouvoir rendre le b\\u00e9n\\u00e9fice de ce moteur aux acteurs priv\\u00e9s par le fait de remplacer la charte graphique de l''Etat par une charte g\\u00e9n\\u00e9rique, des chartes g\\u00e9n\\u00e9riques. Voici le lieu de notre travail:</p><p data-block-key=\\\"9fir4\\\"><a href=\\\"https://github.com/chris2fr/sites-faciles\\\">https://github.com/chris2fr/sites-faciles</a></p><p data-block-key=\\\"f6rl6\\\">Les choix prix par le DINUM, \\u00e0 partir du m\\u00eame socle technologique des Grands Voisins -- Wagtail, Django, Python -- sont structurants et rationnels. Le fait d''ouvrir ces d\\u00e9veloppements \\u00e0 un grand nombre nous permet de reverser \\u00e0 l''Etat nos contributions modestes. C''est un exemple de la puissance du dynamique des logiciels libres.</p><p data-block-key=\\\"8la9t\\\">Ce site en travaux est disponible sur <a href=\\\"https://facile.lesgrandsvoisins.com\\\">https://facile.lesgrandsvoisins.com</a>.</p><p data-block-key=\\\"fmm91\\\">Nous avons aussi essay\\u00e9 un site statique sur <a href=\\\"https://les.gv.coop\\\">https://les.gv.coop</a></p><p data-block-key=\\\"3djgc\\\">Le site actuel est sur <a href=\\\"https://www.lesgrandsvoisins.com\\\">https://www.lesgrandsvoisins.com</a></p>\", \"id\": \"87661a48-af93-4913-b933-f0afb4d285b7\"}, {\"type\": \"stepper\", \"value\": {\"title\": \"Mise en place du nouveau site web sous Sites-Faciles\", \"total\": 3, \"current\": 1, \"steps\": [{\"type\": \"step\", \"value\": {\"title\": \"Installation et tests\", \"detail\": \"Il s''agit d''installer le dispositif depuis sites-faciles, de faire des tests et d''ajuster le look-and-feel.\"}, \"id\": \"9a7f37fb-22e6-4878-80cd-8f8e31925334\"}, {\"type\": \"step\", \"value\": {\"title\": \"Validation et contenu\", \"detail\": \"Il s''agit d''organiser l''information et affiner la pr\\u00e9sentation avec une exp\\u00e9rience utilisateur convenable.\"}, \"id\": \"19234db7-2154-489c-9c57-d116199345c8\"}, {\"type\": \"step\", \"value\": {\"title\": \"Installation et mise en production\", \"detail\": \"Il s''agit de faire en sorte que le site web se charge rapidement et est conforme aux besoins de l''association et du Monde.\"}, \"id\": \"48947ec7-5ab0-48a9-8957-d813fc032715\"}]}, \"id\": \"1eea2c01-8d52-4398-8626-9f5298487fc0\"}]", "date": "2024-05-06T08:37:00Z", "live": true, "path": "000100020003000O", "slug": "sites-faciles", "depth": 4, "owner": 1, "title": "Refonte web avec Sites Faciles", "locale": 1, "locked": false, "authors": [1], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/sites-faciles/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "Refonte web avec avec Sites Faciles", "entry_tags": [{"pk": null, "tag": 34, "content_object": 34}, {"pk": null, "tag": 35, "content_object": 34}, {"pk": null, "tag": 36, "content_object": 34}], "go_live_at": null, "draft_title": "Refonte web avec Sites Faciles", "content_type": 71, "header_image": 53, "header_large": true, "header_darken": false, "live_revision": 82, "show_in_menus": true, "blog_categories": [2], "header_cta_link": null, "header_cta_text": null, "latest_revision": 91, "translation_key": "53775bc0-3da8-4a48-b082-d4b2cd50bd6b", "entry_categories": [{"pk": 26, "page": 34, "category": 2}], "header_cta_label": null, "header_with_title": false, "last_published_at": "2024-05-06T08:48:00.286Z", "first_published_at": "2024-05-06T08:45:06.274Z", "header_color_class": "grey", "search_description": "La DINUM a eu la grce de publier son moteur CMS pour numerique.gouv.fr sur github. Nous nous donnons comme mission []", "wagtail_admin_comments": [], "has_unpublished_changes": true, "latest_revision_created_at": "2024-05