INSERT INTO public.wagtailcore_revision VALUES (27, '2024-05-05 17:11:32.649809+00', '{"pk": 12, "body": "[{\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"vz7ve\\\">Voici les premiers mots de Thimot\\u00e9 sur un ordinateur</p><p data-block-key=\\\"f50ls\\\">Salut mon ami comment vas-tu ? Si tout va bien, DIEU merci. Et les activit\\u00e9s \\u00e7a marche bien ou pas ? Cela \\u00a0fait un bon moment qu\\u2019on ne se voit pas. J\\u2019esp\\u00e8re qu\\u2019on se reverra tr\\u00e8s prochainement.</p><p data-block-key=\\\"649qv\\\">Et Timoth\\u00e9e revient deux semaines plus tard pour ajouter \\u00e0 l''article.</p><p data-block-key=\\\"3iblt\\\">Bonjour les Grands Voisins,</p><p data-block-key=\\\"5mrg4\\\">Je suis ravi d\\u2019entendre parler de vous. je suis encore l\\u00e0 aujourd\\u2019hui pour me connecter au monde de l\\u2019informatique, du num\\u00e9rique. L\\u2019atelier d\\u2019entraide num\\u00e9rique est vraiment important et la ressourcerie cr\\u00e9ative comme son nom l\\u2019indique est une chose a d\\u00e9couvrir \\u00e9tant donn\\u00e9 que c\\u2019est vraiment un endroit pour se ressourcer et cr\\u00e9er.</p><p data-block-key=\\\"6nhgc\\\">Ne dit-on pas que l\\u2019analphab\\u00e8te du 21 si\\u00e8cle n\\u2019est plus celui qui n\\u2019a jamais \\u00e9t\\u00e9 a l\\u2019\\u00e9cole mais c\\u2019est celui qui n\\u2019a pas de notion en informatique.</p>\", \"id\": \"e1c2a258-883d-4473-b38b-6c715235f8fb\"}, {\"type\": \"image\", \"value\": {\"title\": \"Timoth\\u00e9e revient et tape tout seul\", \"heading_tag\": \"h3\", \"image\": 18, \"alt\": \"Timoth\\u00e9e revient et tape tout seul\", \"caption\": \"\", \"url\": \"\"}, \"id\": \"5fc5066d-aa4f-4535-aca9-56b0b6ab9d2e\"}]", "date": "2024-05-05T17:06:00Z", "live": true, "path": "0001000200030006", "slug": "des-premiers-mots", "depth": 4, "owner": 1, "title": "Des premiers mots", "locale": 1, "locked": false, "authors": [], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/des-premiers-mots/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "Des premiers mots", "entry_tags": [], "go_live_at": null, "draft_title": "Des premiers mots", "content_type": 71, "header_image": 19, "header_large": true, "header_darken": true, "live_revision": 26, "show_in_menus": true, "blog_categories": [], "header_cta_link": null, "header_cta_text": null, "latest_revision": 26, "translation_key": "f58ba4f8-4a5b-4aed-a2e5-517d270e4503", "entry_categories": [], "header_cta_label": null, "header_with_title": true, "last_published_at": "2024-05-05T17:11:01.930Z", "first_published_at": "2024-05-05T17:11:01.930Z", "header_color_class": "blue-cumulus", "search_description": "Voici les premiers mots de Thimot sur un ordinateur Salut mon ami comment vas-tu ? Si tout va bien, DIEU []", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": "2024-05-05T17:11:01.908Z"}', NULL, '12', 1, 71, 1, 'Des premiers mots');
INSERT INTO public.wagtailcore_revision VALUES (35, '2024-05-05 17:39:31.540126+00', '{"pk": 17, "body": "[{\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"ytcaw\\\">Participez \\u00e0 ces ateliers conviviaux d\\u2019entraide num\\u00e9rique<br/>pour vous-m\\u00eame ou pour votre organisation. Vous pouvez y avancer vos projets et vos savoir-faire aussi en aidant d\\u2019autres avec les leurs.</p><p data-block-key=\\\"8etl9\\\">Tous niveaux, tous \\u00e2ges, tous besoins sont les bienvenus :<br/>+ des craintifs du num\\u00e9rique aux ing\\u00e9nieurs informaticiens ;<br/>+ des sans ordinateurs fixes aux \\u00e9quip\\u00e9s du nec plus ultra ;<br/>+ des h\\u00e9berg\\u00e9s ou migrants aux propri\\u00e9taires ou entreprises du 14e ;<br/>+ des salari\\u00e9s ou non-salari\\u00e9s aux patrons du 14e ;<br/>+ des \\u00e9tudiants aux retrait\\u00e9s ; etc.</p><p data-block-key=\\\"ahk8k\\\"><b>Le lundi de 16h \\u00e0 19h se tient un atelier d\\u2019entraide num\\u00e9rique</b><br/><b>\\u00e0 l\\u2019Atelier de la Ressourcerie Cr\\u00e9ative</b> au 93 ave. du G\\u00e9n. Leclerc et <b>en ligne \\u00e0</b> <a href=\\\"https://meet.lesgrandsvoisins.com/?ref=blog.lesgrandsvoisins.com\\\"><b>meet.lesgrandsvoisins.com</b></a>.<br/>En plus de vous-m\\u00eames et vos souhaits, vous pouvez amener votre \\u00e9quipement (ordinateur, tablette, smartphone, \\u2026), \\u00e9ventuellement un truc \\u00e0 manger et/ou \\u00e0 boire (sans alcool). Nous aurions des ordinateurs sur place et \\u00e9ventuellement d\\u2019autres choses..</p><p data-block-key=\\\"catpb\\\">Programmation mensuelle \\u00e0 explorer : Fondation Valentin Ha\\u00fcy 3 Rue Jacquier (\\u00e9quipement pour mal-voyants) ; La Table des Mati\\u00e8res 51 Rue de l''Abb\\u00e9 Carton ; peut-\\u00eatre la Maison des r\\u00e9fugi\\u00e9s, 50 Bd Jourdan ; Ailleurs ? (la salle est transportable)</p><p data-block-key=\\\"4m4ae\\\">Lancement b\\u00eata \\u00e0 partir du lundi 7 ao\\u00fbt, d\\u00e9finitif \\u00e0 partir du lundi 4 septembre.<br/>Contact chris@lesgrandsvoisins.com au 07 81 81 18 11 pour toute question.<br/><a href=\\\"https://www.lesgrandsvosins.com/resdigita?ref=blog.lesgrandsvoisins.com\\\">https://www.lesgrandsvosins.com/resdigita</a></p>\", \"id\": \"ecb4d132-119b-4148-8d3c-68dbfd7bd6ee\"}]", "date": "2023-06-30T12:00:00Z", "live": true, "path": "000100020003000B", "slug": "salle-de-sociabilite-numerique", "depth": 4, "owner": 1, "title": "Salle de sociabilit numrique", "locale": 1, "locked": false, "authors": [1], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/salle-de-sociabilite-numerique/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "Salle de sociabilit numrique", "entry_tags": [{"pk": 25, "tag": 24, "content_object": 17}, {"pk": 26, "tag": 15, "content_object": 17}], "go_live_at": null, "draft_title": "Salle de sociabilit numrique", "content_type": 71, "header_image": 26, "header_large": true, "header_darken": true, "live_revision": null, "show_in_menus": true, "blog_categories": [2], "header_cta_link": null, "header_cta_text": null, "latest_revision": null, "translation_key": "418f8148-8bb7-4dcf-b70b-9055cdb42150", "entry_categories": [{"pk": 13, "page": 17, "category": 2}], "header_cta_label": null, "header_with_title": true, "last_published_at": null, "first_published_at": null, "header_color_class": "green-archipel", "search_description": "Participez  ces ateliers conviviaux dentraide numrique\r\npour vous-mme ou pour votre organisation. Vous pouvez y avancer vos projets et vos savoir-faire aussi en aidant dautres avec les leurs.", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": null}', NULL, '17', 1, 71, 1, 'Salle de sociabilit numrique');
