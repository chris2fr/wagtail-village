INSERT INTO public.sites_faciles_contentpage VALUES (116, '[{"id": "e902d99c-9a08-4345-a5fa-365c350f26f2", "type": "paragraph", "value": "<p data-block-key=\"cwh4c\">La journe du 10 mai 2023, l''INCLUSIV''DAY s''est droule  la Dfense, el Manhattan de PARIS avec la prsence de deux ministres du government Macron. J''y avais un stand pour LesGrandsVoisins.com.</p><p data-block-key=\"4t7v4\">La France a enfin reconnu mon ct  Mejedhoob  RQTH ayant moi-mme un pet au casque qui risque de dgainer sur la politique extrieure de mon paye si mes droits, en tant qu''ambassadeur auto-proclam de l''art contempoain de Mauritanie en occident n''est pas reconnu et respect par l''Etat Mauritanien.</p><p data-block-key=\"d990l\">Ma rencontre ubuesque avec les deux ministres dnote de la diplomatie parallle active que je mne, en solo, depuis des dcennies sans le moindre \"encouragement\" des hautes sphres de mon pays, juste parce-que je suis un homme libre des milieux nantis et bandits: \"Allah Yenal Sow\" !</p><p data-block-key=\"25uub\">maelanc.com</p><p data-block-key=\"23s6t\">Mal ANC</p>"}]', ,, 25, 'yellow-tournesol', NULL, NULL, true, true, true, NULL));
