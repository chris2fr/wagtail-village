INSERT INTO public.sites_faciles_contentpage VALUES (134, '[{"id": "87661a48-af93-4913-b933-f0afb4d285b7", "type": "paragraph", "value": "<p data-block-key=\"yzz1i\">La DINUM a eu la grce de publier son moteur CMS pour numerique.gouv.fr sur github. Nous nous donnons comme mission de pouvoir rendre le bnfice de ce moteur aux acteurs privs par le fait de remplacer la charte graphique de l''Etat par une charte gnrique, des chartes gnriques. Voici le lieu de notre travail:</p><p data-block-key=\"9fir4\"><a href=\"https://github.com/chris2fr/sites-faciles\">https://github.com/chris2fr/sites-faciles</a></p><p data-block-key=\"f6rl6\">Les choix prix par le DINUM,  partir du mme socle technologique des Grands Voisins -- Wagtail, Django, Python -- sont structurants et rationnels. Le fait d''ouvrir ces dveloppements  un grand nombre nous permet de reverser  l''Etat nos contributions modestes. C''est un exemple de la puissance du dynamique des logiciels libres.</p><p data-block-key=\"8la9t\">Ce site en travaux est disponible sur <a href=\"https://facile.lesgrandsvoisins.com\">https://facile.lesgrandsvoisins.com</a>.</p><p data-block-key=\"fmm91\">Nous avons aussi essay un site statique sur <a href=\"https://les.gv.coop\">https://les.gv.coop</a></p><p data-block-key=\"3djgc\">Le site actuel est sur <a href=\"https://www.lesgrandsvoisins.com\">https://www.lesgrandsvoisins.com</a></p>"}, {"id": "1eea2c01-8d52-4398-8626-9f5298487fc0", "type": "stepper", "value": {"steps": [{"id": "9a7f37fb-22e6-4878-80cd-8f8e31925334", "type": "step", "value": {"title": "Installation et tests", "detail": "Il s''agit d''installer le dispositif depuis sites-faciles, de faire des tests et d''ajuster le look-and-feel."}}, {"id": "19234db7-2154-489c-9c57-d116199345c8", "type": "step", "value": {"title": "Validation et contenu", "detail": "Il s''agit d''organiser l''information et affiner la prsentation avec une exprience utilisateur convenable."}}, {"id": "48947ec7-5ab0-48a9-8957-d813fc032715", "type": "step", "value": {"title": "Installation et mise en production", "detail": "Il s''agit de faire en sorte que le site web se charge rapidement et est conforme aux besoins de l''association et du Monde."}}], "title": "Mise en place du nouveau site web sous Sites-Faciles", "total": 3, "current": 1}}, {"id": "70854f40-8c96-413f-b377-d9a1d1b0435a", "type": "multicolumns", "value": {"title": "", "columns": [{"id": "bb0d1e3a-a505-44bd-ba0b-3663b6ab2b06", "type": "text", "value": "<h3 data-block-key=\"l1q4l\">Appropriation du dispositif de la DINUM</h3><p data-block-key=\"5d2ak\">Le dispositif <a href=\"https://sites-faciles.beta.numerique.gouv.fr/\">sites-faciles</a> de la DINUM est trs troitement li aux graphismes de l''Etat, mais a aussi des choix pratiques et communicatives pour des sites webs en France. Nous avons enregistrs <a href=\"https://github.com/numerique-gouv/sites-faciles/issues\">quelques items</a> avec la DINUM pour travailler sur ces sujets.</p>"}, {"id": "1d3be829-86b6-4bd6-93a8-64a11555df9f", "type": "text", "value": "<h3 data-block-key=\"00gq4\">Cration des contenus des GV</h3><p data-block-key=\"8f2nt\">Nous avons des contenus sur <a href=\"https://wagtail.lesgrandsvoisins.com\">wagtail.resdigita.com</a> et sur <a href=\"https://blog.lesgrandsvoisins.com\">blog.lesgrandsvoisins.com</a>  migrer sur ce nouveau dispositif.</p>"}], "bg_color": "", "bg_image": null, "heading_tag": "h2", "bg_color_class": ""}}]', ,, 53, 'grey', NULL, NULL, false, true, false, NULL));
