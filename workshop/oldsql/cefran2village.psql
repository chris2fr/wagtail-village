INSERT INTO public.wagtailcore_revision VALUES (111, '2024-05-08 16:22:25.492984+00', '{"pk": 40, "body": "[{\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"0xk8q\\\">Ce manifeste, imparfait, refl\\u00e8te les esp\\u00e9rances, les souffrances et les incompr\\u00e9hensions de ceux qui l''ont insuffl\\u00e9. Les Grands Voisins sont n\\u00e9s \\u00e0 l''h\\u00f4pital Saint-Vincent-de-Paul dans le 14e arrondissement \\u00e0 Paris, entre Port royal et les Catacombes. Nous sommes issus des failles dans le b\\u00e9ton et dans les politiques publiques. Nous sommes au service de l''accueil de tous et croyons aux ponts entre utopie et r\\u00e9alit\\u00e9.</p><p data-block-key=\\\"fmap5\\\">Comme toutes bonnes voisines et tous bons voisins, nous ne sommes pas toujours d''accord entre nous, mais nous d\\u00e9fendons :</p><ul><li data-block-key=\\\"78ndh\\\">la parit\\u00e9 dans la gouvernance</li><li data-block-key=\\\"jv61\\\">des solutions collectives en bonne intelligence</li><li data-block-key=\\\"ahscr\\\">l''expression libre</li><li data-block-key=\\\"cehns\\\">l''exercice artistique</li><li data-block-key=\\\"8fpaf\\\">l''apprentissage tout au long de la vie</li><li data-block-key=\\\"4ctm8\\\">la valorisation de toutes formes de travail.</li></ul><p data-block-key=\\\"72jpr\\\">Aux Grands Voisins, nous vivons, travaillons, dormons, mangeons, nous soignons, achetons, pleurons, rions, et tout cela dans toutes les langues. L''inclusion et l''exclusion, toutes les deux, peuvent nous \\u00e9loigner de l''Autre qui est une chance.</p><p data-block-key=\\\"e6llj\\\">Nous r\\u00e9clamons des lieux de mixit\\u00e9 sociale qui permettent la valorisation et le partage des savoirs et des savoir-faire de toutes et de tous. Nous r\\u00e9clamons des lieux de mixit\\u00e9 sociale qui favorisent l''\\u00e9coute et les possibilit\\u00e9s de rencontre, conditions visant \\u00e0 l''abolition de la peur de l''autre. Nous r\\u00e9clamons des lieux de mixit\\u00e9 sociale qui luttent contre \\u00ab l''entre-soi \\u00bb, l''isolement et la r\\u00e9duction des individus \\u00e0 des cases.</p><p data-block-key=\\\"6obpf\\\">Nous avons besoin de lieux et de temps pour faire pr\\u00e9c\\u00e9dent, pouvoir inventer d''autres formes de soci\\u00e9t\\u00e9 en harmonie avec les \\u00e9cosyst\\u00e8mes. Nous avons toutes et tous \\u00e0 donner et nous pouvons toutes et tous recevoir.</p><p data-block-key=\\\"4253n\\\"><i>[Manifeste des Grands Voisins avec les contributions d''une centaine de personnes sur un mois dont cette version est \\u00e9dit\\u00e9e par Chris Mann en 2017 avec une r\\u00e9vision l\\u00e9g\\u00e8re en 2023.]</i></p>\", \"id\": \"79b8666c-64a7-44d2-9a77-7a02649f668a\"}]", "date": "2023-04-02T10:21:00Z", "live": true, "path": "000100020003000R", "slug": "manifeste-des-grands-voisins", "depth": 4, "owner": 1, "title": "Manifeste des Grands Voisins", "locale": 1, "locked": false, "authors": [1], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/manifeste-des-grands-voisins/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "Manifeste des Grands Voisins", "entry_tags": [], "go_live_at": null, "draft_title": "Manifeste des Grands Voisins", "content_type": 71, "header_image": 48, "header_large": true, "header_darken": true, "live_revision": null, "show_in_menus": true, "blog_categories": [3], "header_cta_link": null, "header_cta_text": null, "latest_revision": null, "translation_key": "f3fccd41-4c4e-4861-b048-dd29715c3774", "entry_categories": [{"pk": 28, "page": 40, "category": 3}], "header_cta_label": null, "header_with_title": true, "last_published_at": null, "first_published_at": null, "header_color_class": null, "search_description": "Version du 2 avril 2022 du manifeste des Grands Voisins compos en 2017 avec plus de cent personnes sur un priode d''un mois.", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": null}', NULL, '40', 1, 71, 1, 'Manifeste des Grands Voisins');
