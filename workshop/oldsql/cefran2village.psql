INSERT INTO public.sites_faciles_contentpage VALUES (48, '[{"id": "67c34257-588f-4b11-8055-bb10287eb3c8", "type": "imageandtext", "value": {"link": {"page": null, "text": "", "external_url": ""}, "page": null, "text": "<p data-block-key=\"zqptc\">Sur la base de l''application gestionnaire de contenu CMS et blog sous technologie Wagtail <a href=\"https://github.com/numerique-gouv/sites-faciles\">sites-faciles de numerique.gouv.fr</a>, elle-mme  base de l''application de mise en page de site dynamique Django <a href=\"https://github.com/numerique-gouv/django-dsfr\">django-dsfr</a>, elle mme bas sur le <a href=\"https://github.com/GouvernementFR/dsfr/\">Design Systme de l''Etat ou DSFR du gouvernement franais</a>, cette application gestionnaire de contenu dit CMS et blog <a href=\"https://www.village.org/\">Web Fastoche</a> propose de capitaliser sur le travail de fond pour le traitement d''information et de la communication en rendant les composants gnriques.</p><p data-block-key=\"1ac8v\">Voici <a href=\"https://www.village.org/medias/images/architecture_htM7iUg.original.svg\">un diagramme</a> autour de l''ide de faire un outil publiquement utilisable <a href=\"https://github.com/chris2fr/wagtail-village\">Wagtail-Fastoche</a> avec <a href=\"https://github.com/chris2fr/django-village\">Django-Fastoche</a> <a href=\"https://pypi.org/project/django-village/\">Module Python Django-Fastoche</a> et <a href=\"https://github.com/chris2fr/fabrique-village\">Fabrique-Fastoche</a>.</p>", "image": 62, "link_url": "", "image_side": "right", "link_label": "", "image_ratio": "6"}}]', NULL, NULL, NULL, false, NULL, false, false, NULL);
INSERT INTO public.sites_faciles_contentpage VALUES (51, '[{"id": "c58f3abe-971d-41a7-ad49-f547da49dd04", "type": "multicolumns", "value": {"title": "", "columns": [{"id": "81d80008-6286-46b4-800e-edf8d4fce080", "type": "text", "value": "<h3 data-block-key=\"wopaa\">22nd Conseil of Les Grands Voisins</h3><p data-block-key=\"9sn6r\"></p><embed alt=\"flyer-20240506-conseildesgv22\" embedtype=\"image\" format=\"left\" id=\"54\"/><p data-block-key=\"4jn74\"></p><p data-block-key=\"f38i5\">Nous tiendrions le 22e Conseil des Voisins sur trois rencontres :</p><ul><li data-block-key=\"sqrq\">Lundi le 10 juin de 18h  19h30</li><li data-block-key=\"5sba\">Jeudi le 13 juin de 19h  20h30</li><li data-block-key=\"fabkv\">Samedi le le 15 juin de 10h30  midi</li></ul><p data-block-key=\"a6q9u\">Ordre du jour :</p><ol><li data-block-key=\"43o40\">Numrique</li><li data-block-key=\"oioh\">ventuellement Civisme</li><li data-block-key=\"7l6q8\">Communication</li></ol>"}, {"id": "460c495c-476c-43f1-bb69-aff7bf1509ea", "type": "text", "value": "<h3 data-block-key=\"avd09\"><b>Permanence le lundi de midi  17h</b></h3><p data-block-key=\"e0hvf\"></p><embed alt=\"Abaker-Atelier\" embedtype=\"image\" format=\"left\" id=\"52\"/><p data-block-key=\"5c5gg\"></p><p data-block-key=\"dqj9a\">Venez  notre permanence <b>le lundi de 12h  17h</b><br/>en personne  <b>93 ave. Du Gn. Leclerc</b> 75014 Paris (Mtro Alsia, Atelier de la Ressourcerie, <i>7e boutique sur la gauche</i>) ou <a href=\"https://meet.lesg/\">meet.lesgrandsvoisins.com pour participer en hybride</a>.</p>"}, {"id": "6fc069f3-372a-447a-8d33-fd26389bdac8", "type": "text", "value": "<h3 data-block-key=\"avd09\"><a href=\"https://www.lesgrandsvoisins.fr/numerique/numerique-creatif/sociabilite-numerique/\"><b>Sociabilit numrique le lundi de 17h  21h30</b></a></h3><embed alt=\"numasso\" embedtype=\"image\" format=\"right\" id=\"22\"/><p data-block-key=\"cda90\"><b>Le lundi de 17h  21h30</b> se tient un atelier dentraide numrique  lAtelier au <b>93 ave. du Gn. Leclerc 75014 PARIS</b>  lintrieur du <i>centre commercial 7e boutique sur la gauche</i></p>"}], "bg_color": "", "bg_image": null, "heading_tag": "h2", "bg_color_class": ""}}, {"id": "d3e4fb2e-7b28-4da0-99d3-09c2868a013f", "type": "paragraph", "value": "<p data-block-key=\"q7an3\"><a href=\"https://list.lesgrandsvoisins.com/subscription/form\"><b><i>Pensez  vous inscrire sur notre liste d''infos !</i></b></a></p><p data-block-key=\"evpog\">Nous sommes une association d''intrt gnral agissant de manire contrle en subsidiarit ouverte pour toute organisation et tout individu qui en fait la dcision pour soi-mme principalement en ces trois domaines <a href=\"https://www.lesgrandsvoisins.fr/communautes/\">de Discussions</a>, <a href=\"https://www.lesgrandsvoisins.fr/arts/\">d''Arts plastiques</a> et <a href=\"https://www.lesgrandsvoisins.fr/numerique/\">de Numrique</a> selon <a href=\"https://www.lesgrandsvoisins.fr/mission/\">notre dclaration de mission</a>.</p><p data-block-key=\"bj8u6\"><a href=\"https://www.lesgrandsvoisins.fr/communautes/\"><b>Discussions</b></a> :</p><ul><li data-block-key=\"61obe\">Nous proposons des conditions encourageant la comprhension entre individus et organisations de perspectives diffrentes.</li><li data-block-key=\"9oi9e\">Nous ouvrons notre gouvernance pour des votes en Conseils des Voisins.</li></ul><p data-block-key=\"65va7\"><a href=\"https://www.lesgrandsvoisins.fr/arts/\"><b>Arts plastiques</b></a> :</p><ul><li data-block-key=\"a2qag\">Nous encourageons lutilisation hybrides despace (commerces, hpitaux, cafs, etc.) aussi en gallries locales.</li><li data-block-key=\"2ek3r\">Nous accompagnons des institutions pour trouver une juste place pour les arts plastiques dans leurs locaux.</li></ul><p data-block-key=\"91unk\"><a href=\"https://www.lesgrandsvoisins.fr/numerique/\"><b>Numrique</b></a> :</p><ul><li data-block-key=\"afesu\">Nous maintenons une constellation de serveurs et de services internet -- tableau de bord, cration et gestion de compte, courriel, rencontres hybrides prsence-distance, listes de diffusion numriques, blogs, serveurs webs, gestionnaire de mot de passe, enregistrement dans le nuage, forums de discussion, gestion de la connaissance, dition markdown, wikis, dition bueautique, etc. -- pour, par, et de nous-mmes et d''autres.</li><li data-block-key=\"ej6ca\">Nous mettons  disposition des talents et des matriaux dans des salles de sociabilit numrique et dans des systmes de prts et changes de matriels  l''attention d''individus et d''organisations de tous niveaux.</li></ul><p data-block-key=\"apjqo\">Voici quelques entits Grandes Voisines qui soutiennent notre dmarche: A Good Solution, Banya Tour, Robert Baldridge, BMCP, Breast Freedom, Le Bureau du Crateur, Le Chiffon, Les Jardins du Numrique, Olivier Duhec, Inno Cube, Caroline Lhomme, Francesco du 59 Rivoli, Amadou Gaye, Informatruck, Inolib, KaceKode, Ada Matus, LOdyse, La Ressourcerie Crative, Henry Roy, Peter Vaeth, Waka Conseil, Weezem, et peut-tre des vtres </p><p data-block-key=\"at7dp\">Pour plus dinformations, contactez-nous: chris@lesgrandsvoisins.com +33 7 81 811 811</p><p data-block-key=\"4coel\"></p>"}, {"id": "f53195d5-0de5-479d-aef1-6151fbd266e0", "type": "video", "value": {"url": "https://www.youtube.com/embed/qZqErGfNF_o?feature=oembed", "title": "Les Grands Voisins, la cit rve", "caption": "c 3, 2020 Ralisation : Bastien Simon  Pitch : Mal, artiste sans papier, Adrien, luthier musicien et d''autres rsidents venus de tous horizons, s''organisent pour donner naissance  une utopie moderne en plein coeur de Paris, un village solidaire de prs de 2000 personnes : Les Grands Voisins.  travers leurs trajectoires et celles des membres fondateurs du lieu, le film interroge notre dsir et notre capacit  inventer dautres manires de vivre ensemble. Que retiendrons-nous de cette exprience collective ? Pourra-t-elle perdurer, essaimer, rsonner ailleurs ?"}}, {"id": "03449e53-e17e-4ae3-ae63-7d9140be0b08", "typ