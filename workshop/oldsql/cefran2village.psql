INSERT INTO public.wagtailcore_revision VALUES (19, '2024-05-05 16:33:24.278372+00', '{"pk": 7, "body": "[{\"type\": \"imageandtext\", \"value\": {\"image\": 10, \"image_side\": \"right\", \"image_ratio\": \"3\", \"text\": \"<p data-block-key=\\\"8vslz\\\"><i>Changeons le monde ensembles.</i></p><p data-block-key=\\\"cbep\\\">Nous proposons notre 21e Conseil des Voisins le 28 avril \\u00e0 10h dans le 14e arrondissement \\u00e0 Paris et en ligne sur <a href=\\\"https://meet.lesgrandsvoisins.com/?ref=blog.lesgrandsvoisins.com\\\">meet.lesgrandsvoisins.com</a></p><p data-block-key=\\\"7c42l\\\">Chez Papa Montparnasse Vavin (RER B Port Royal ou Metro 4 Vavin)<br/>138 Boulevard du Montparnasse, 75014 Paris</p><p data-block-key=\\\"3h4r2\\\">(<a href=\\\"https://maps.app.goo.gl/9F1w2Fam8goZ3ASx9?ref=blog.lesgrandsvoisins.com\\\">lien Maps</a>)</p><p data-block-key=\\\"aqqh1\\\">Ordre du jour :</p><ol><li data-block-key=\\\"d29qa\\\">Comptes annuels (faibles)</li><li data-block-key=\\\"9oc8p\\\">Activit\\u00e9s : arts plastiques, num\\u00e9rique, civisme</li><li data-block-key=\\\"4fef0\\\">Communication</li></ol><p data-block-key=\\\"6p2dh\\\">Nous parlerions des sujets pr\\u00e9cis en lien avec la m\\u00e9moire de Valentin FRANCY, le num\\u00e9rique cr\\u00e9atif des Grands Voisins (Budget Participatif de Paris), efforts pour rendre des h\\u00f4pitaux hybrides aussi en galeries d''art.</p><p data-block-key=\\\"ahns\\\"><a href=\\\"https://www.instagram.com/les_grands_voisins/?utm_source=ig_embed&amp;ig_rid=f6153464-ce6a-41dd-b392-5fc008623cfa\\\">https://www.instagram.com/les_grands_voisins/?utm_source=ig_embed&amp;ig_rid=f6153464-ce6a-41dd-b392-5fc008623cfa</a></p><p data-block-key=\\\"265ce\\\"></p>\", \"link\": {\"page\": null, \"external_url\": \"\", \"text\": \"\"}, \"link_label\": \"\", \"page\": null, \"link_url\": \"\"}, \"id\": \"66ff0747-1ccf-4761-8adc-4a805dd4e883\"}]", "date": "2024-04-19T08:00:00Z", "live": true, "path": "0001000200030001", "slug": "21e-conseil-des-grands-voisins", "depth": 4, "owner": 1, "title": "21e Conseil des Grands Voisins", "locale": 1, "locked": false, "authors": [1], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/21e-conseil-des-grands-voisins/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "21e Conseil des Grands Voisins", "entry_tags": [{"pk": null, "tag": 9, "content_object": 7}, {"pk": null, "tag": 10, "content_object": 7}], "go_live_at": null, "draft_title": "21e Conseil des Grands Voisins", "content_type": 71, "header_image": 9, "header_large": true, "header_darken": true, "live_revision": 18, "show_in_menus": true, "blog_categories": [3], "header_cta_link": null, "header_cta_text": null, "latest_revision": 18, "translation_key": "bafd634b-2f39-48a2-95a9-d781aa3eb7a1", "entry_categories": [{"pk": 1, "page": 7, "category": 3}], "header_cta_label": null, "header_with_title": true, "last_published_at": "2024-05-05T16:29:53.465Z", "first_published_at": "2024-05-05T16:26:44.202Z", "header_color_class": "blue-france", "search_description": "Ordre du jour :\r\n\r\nComptes annuels (faibles)\r\nActivits : arts plastiques, numrique, civisme\r\nCommunication\r\nNous parlerions des sujets prcis en lien avec la mmoire de Valentin FRANCY, le numrique cratif des Grands Voisins (Budget Participatif de Paris), efforts pour rendre des hpitaux hybrides aussi en galeries d''art.", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": "2024-05-05T16:29:53.438Z"}', NULL, '7', 1, 71, 1, '21e Conseil des Grands Voisins');
INSERT INTO public.wagtailcore_revision VALUES (22, '2024-05-05 16:53:03.91299+00', '{"pk": 8, "body": "[{\"type\": \"callout\", \"value\": {\"title\": \"\", \"text\": \"Proposition de communiqu\\u00e9 pour la D\\u00e9l\\u00e9gation permanente de la R\\u00e9publique Islamique de Mauritanie \\u00e0 l\\u2019UNESCO \\u00e0 l\\u2019occaison du retour des Etats Unis par l\\u2019artiste Mohamed Ma\\u00ebl A\\u00efnine Sidi Ethmane.\", \"heading_tag\": \"h3\"}, \"id\": \"2bb8edae-aac8-4208-bb68-677a9e5e0b00\"}, {\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"vq1lp\\\">Paris, le 14 juitllet 2023</p><p data-block-key=\\\"bc251\\\">Ma\\u00ebl A.N.C.<br/>22 Rue Duranton,<br/>75015 Paris,</p><p data-block-key=\\\"eq7hr\\\">Proposition de communiqu\\u00e9 pour la \\u00a0D\\u00e9l\\u00e9gation permanente de la R\\u00e9publique Islamique de Mauritanie \\u00e0 l\\u2019UNESCO \\u00e0 l\\u2019occaison du retour des Etats Unis par l\\u2019artiste Mohamed Ma\\u00ebl A\\u00efnine Sidi Ethmane.</p>\", \"id\": \"a1d22914-8142-4f56-8195-acceaa062969\"}, {\"type\": \"separator\", \"value\": {\"top_margin\": 3, \"bottom_margin\": 3}, \"id\": \"1ff0b627-f8af-403a-86ee-7c48dceeca4d\"}, {\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"vq1lp\\\">La D\\u00e9l\\u00e9gation permanente de la R\\u00e9publique Islamique de Mauritanie \\u00e0 l''UNESCO</p><p data-block-key=\\\"1ib9p\\\"></p><embed embedtype=\\\"image\\\" format=\\\"fullwidth\\\" id=\\\"13\\\" alt=\\\"\\\"/><p data-block-key=\\\"ebc01\\\"></p><p data-block-key=\\\"abvac\\\">f\\u00e9licite avec gratitude les \\u00c9tats Unis d\\u2019Am\\u00e9rique</p><p data-block-key=\\\"405oi\\\"></p><embed embedtype=\\\"image\\\" format=\\\"fullwidth\\\" id=\\\"14\\\" alt=\\\"flag-usa.pmg\\\"/><p data-block-key=\\\"6vria\\\">de leur retour historique \\u00e0 L\\u2019UNESCO</p><p data-block-key=\\\"fan0p\\\"></p><embed embedtype=\\\"image\\\" format=\\\"fullwidth\\\" id=\\\"12\\\" alt=\\\"\\\"/><p data-block-key=\\\"8hpaf\\\">Nous sommes fiers et conquis par l\\u2019excellent truchement politique de bon aloi de la Directrice G\\u00e9n\\u00e9rale de l\\u2019UNESCO, Madame Audrey Azouley, qui a dignement conduit la r\\u00e9ussite de cet \\u00e9v\\u00e8nement majeur dans l\\u2019histoire de notre organisation onusienne.</p><p data-block-key=\\\"d2qnj\\\">Nous nous joignons \\u00e0 la solidarit\\u00e9 inconditionnelle de tous les \\u00c9tats membres de L\\u2019UNESCO pour accueillir ce grand retour de nos amis communs des U.S.A.</p><p data-block-key=\\\"55kfk\\\">Merci infiniment, tr\\u00e8s cordialement.</p><p data-block-key=\\\"24unt\\\"></p>\", \"id\": \"a5eb0b1f-d5a6-45ff-ba4a-04b697cfc3ad\"}, {\"type\": \"separator\", \"value\": {\"top_margin\": 3, \"bottom_margin\": 3}, \"id\": \"50667e55-09ac-4afe-925a-4a699179611e\"}, {\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"vq1lp\\\">Je vous prie de consid\\u00e9rer cette expression avec bienveillance.</p><p data-block-key=\\\"fqr5o\\\">Ma\\u00ebl A.N.C.<br/>mael@maelanc.com \\u2013 <a href=\\\"http://www.maelanc.com/?ref=blog.lesgrandsvoisins.com\\\">www.maelanc.com</a></p>\", \"id\": \"3d6b83ce-62cb-4b15-b0ab-13ab6c9a4a5f\"}]", "date": "2023-07-17T13:00:00Z", "live": true, "path": "0001000200030002", "slug": "communique-mauritanie-unesco", "depth": 4, "owner": 1, "title": "Proposition de communiqu pour la Mauritanie  lUNESCO", "locale": 1, "locked": false, "authors": [2], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/communique-mauritanie-unesco/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "Proposition de communiqu pour la Dlgation permanente de la Rpublique Islamique de Mauritanie  lUNESCO", "entry_tags": [], "go_live_at": null, "draft_title": "Proposition de communiqu pour la Mauritanie  lUNESCO", "content_type": 71, "header_image": 12, "header_large": true, "header_darken": true, "live_revision": 21, "show_in_menus": true, "blog_categories": [1, 3], "header_cta_link": null, "header_cta_text": null, "latest_revision": 21, "translation_key": "02059326-e36f-4bd5-b4ae-a29a560e42f4", "entry_categories": [{"pk": 2, "page": 8, "category": 1}, {"pk": 3, "page": 8, "category": 3}], "header_cta_label": null, "header_with_title": true, "last_published_at": "2024-05-05T16:47:37.580Z", "first_published_at": "2024-05-05T16:47:37.580Z", "header_color_class": "red-marianne", "search_description": "Proposition de communiqu pour la Dlgation permanente de la Rpublique Islamique de Mauritanie  lUNESCO  loccaison du retour des Etats Unis par lartiste Mohamed Mal Anine Sidi Ethmane.", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": "2024-05-05T16:47:37.545Z"}', NULL, '8', 1, 71, 1, 'Proposition de communiqu pour la Mauritanie  lUNESCO');
INSERT INTO public.wagtailcore_revision VALUES (25, '2024-05-05 17:06:04.649637+00', '{"pk": 11, "body": "[{\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"ryl3c\\\">Nous allons tenir notre 20e Conseil des voisins <b>le 18 d\\u00e9cembre 2023 de 19h \\u00e0 21h</b>.</p><p data-block-key=\\\"2bddt\\\">Acc\\u00e8s en personne :</p><p data-block-key=\\\"d2eht\\\">Centre commercial \\u00ab Le Square \\u00bb, au 93 ave du G\\u00e9n. Leclerc 75014 Paris (Metro Al\\u00e9sia) 7e boutique sur la gauche.</p><p data-block-key=\\\"dcqhg\\\">Acc\\u00e8s en visio :</p><p data-block-key=\\\"4ln8q\\\"><a href=\\\"https://meet.lesgrandsvoisins.com/?ref=blog.lesgrandsvoisins.com\\\">https://meet.lesgrandsvoisins.com</a> (merci de lire les indications \\u00e0 l''avance)</p><p data-block-key=\\\"8td3s\\\">Un sujet principal de d\\u00e9bat sera notre participation au m\\u00e9morial de Valentin FRANCY le 2 mars 2024, potentiellement conjointement avec Le Souvenir Fran\\u00e7ais. M. FRANCY, un \\u00e9tudiant de 21 ans a \\u00e9t\\u00e9 sauvagement assassin\\u00e9 le 2 mars 2023 par une personne qui \\u00e9tait pr\\u00e9sent \\u00e0 notre 17e Conseil des voisins, puis pour la moiti\\u00e9 de notre 18e Conseil des voisins. M. FRANCY et son assassin avaient pour habitude de s''impliquer dans plusieurs associations en tant que b\\u00e9n\\u00e9voles. A la derni\\u00e8re nouvelle, son assassin confess\\u00e9 est toujours en h\\u00f4pital psychiatrique. Il convient de prendre la juste mesure de cette r\\u00e9alit\\u00e9 violente dans ce monde violent.</p><p data-block-key=\\\"3sldd\\\">D''autres sujets peuvent concerner le rejet surpris par la Mairie de Paris d''honorer le budget participatif Le num\\u00e9rique cr\\u00e9atif des Grands Voisins apr\\u00e8s deux confusions ; notre budget et rapports pour l''ann\\u00e9e qui vient ; un nouveau projet pour les Arts voisins par la transformation d''une salle de r\\u00e9union des familles dans un h\\u00f4pital ; une nouvelle plateforme de fund-raising ; des mani\\u00e8res de pouvoir financer un travail pour le dirigeant ; etc.</p>\", \"id\": \"572bbfce-6e7d-4eae-9496-0d1407896b4e\"}]", "date": "2023-12-03T12:00:00Z", "live": true, "path": "0001000200030005", "slug": "20e-conseil-des-voisins", "depth": 4, "owner": 1, "title": "20e Conseil des Voisins lun. 18 dc. Paris 14e et Visio", "locale": 1, "locked": false, "authors": [1], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/20e-conseil-des-voisins/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "20e Conseil des Voisins lun. 18 dc. Paris 14e et Visio", "entry_tags": [{"pk": 11, "tag": 10, "content_object": 11}, {"pk": 12, "tag": 15, "content_object": 11}], "go_live_at": null, "draft_title": "20e Conseil des Voisins lun. 18 dc. Paris 14e et Visio", "content_type": 71, "header_image": 17, "header_large": true, "header_darken": true, "live_revision": null, "show_in_menus": true, "blog_categories": [3], "header_cta_link": null, "header_cta_text": null, "latest_revision": null, "translation_key": "c607b565-1afb-4811-95b3-977f70662e1b", "entry_categories": [{"pk": 6, "page": 11, "category": 3}], "header_cta_label": null, "header_with_title": true, "last_published_at": null, "first_published_at": null, "header_color_class": "blue-ecume", "search_description": "Nous avons tenu notre 20e Conseil des voisins le 18 dcembre 2023 de 19h  21h.", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": null}', NULL, '11', 1, 71, 1, '20e Conseil des Voisins lun. 18 dc. Paris 14e et Visio');
INSERT INTO public.wagtailcore_revision VALUES (28, '2024-05-05 17:16:00.096786+00', '{"pk": 13, "body": "[{\"type\": \"video\", \"value\": {\"title\": \"Entretien Association HFSDI et Les Grands Voisins\", \"caption\": \"Entretien Association HFSDI et Les Grands Voisins\", \"url\": \"https://www.youtube.com/embed/vRKpUpNo1m8?si=b7BWvcVMqWorMWEb\"}, \"id\": \"7280a5a3-0f73-4ed5-9951-32195ec06b14\"}, {\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"27608\\\">Dans ce vid\\u00e9o, c''est pour tester le principe de l''entretien, Abaker, Pr\\u00e9sident de l''Association Handicap France-Soudan Darfour International, et Chris Mann, Pr\\u00e9sident de l''association Les Grands Voisins, se pr\\u00e9sentent. Puis Chris pose des questions \\u00e0 Abaker et nous apprenons sur l''Association HFSDI</p>\", \"id\": \"d6556e84-1051-4405-b030-44ae3664c8c7\"}]", "date": "2023-10-02T13:00:00Z", "live": true, "path": "0001000200030007", "slug": "essai-video-entretien-handicap-france-soudan-darfour-international", "depth": 4, "owner": 1, "title": "Essai Vido Entretien Handicap France Soudan Darfour International", "locale": 1, "locked": false, "authors": [1], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/essai-video-entretien-handicap-france-soudan-darfour-international/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "Essai Vido Entretien Handicap France Soudan Darfour International", "entry_tags": [{"pk": 13, "tag": 20, "content_object": 13}, {"pk": 14, "tag": 15, "content_object": 13}], "go_live_at": null, "draft_title": "Essai Vido Entretien Handicap France Soudan Darfour International", "content_type": 71, "header_image": 20, "header_large": true, "header_darken": true, "live_revision": null, "show_in_menus": true, "blog_categories": [3, 2], "header_cta_link": null, "header_cta_text": null, "latest_revision": null, "translation_key": "a88d49ba-54d3-4f12-96f2-3824fd53ff80", "entry_categories": [{"pk": 7, "page": 13, "category": 2}, {"pk": 8, "page": 13, "category": 3}], "header_cta_label": null, "header_with_title": true, "last_published_at": null, "first_published_at": null, "header_color_class": "green-emeraude", "search_description": "Lors des essais pour le numrique cratif des Grands Voisins, Abaker et Chris se sont poss dans la Maison des rfugis pour parler de leurs associations respectives Handicap France Soudan Darfour International et Les Grands Voisins.", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": null}', NULL, '13', 1, 71, 1, 'Essai Vido Entretien Handicap France Soudan Darfour International');
INSERT INTO public.wagtailcore_revision VALUES (67, '2024-05-06 03:22:56.047073+00', '{"pk": 30, "body": "[{\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"7uuv2\\\">Ce manifeste, imparfait, refl\\u00e8te les esp\\u00e9rances, les souffrances et les incompr\\u00e9hensions de ceux qui l''ont insuffl\\u00e9. Les Grands Voisins sont n\\u00e9s \\u00e0 l''h\\u00f4pital Saint-Vincent-de-Paul dans le 14e arrondissement \\u00e0 Paris, entre Port royal et les Catacombes. Nous sommes issus des failles dans le b\\u00e9ton et dans les politiques publiques. Nous sommes au service de l''accueil de tous et croyons aux ponts entre utopie et r\\u00e9alit\\u00e9.</p><p data-block-key=\\\"esgvl\\\">Comme toutes bonnes voisines et tous bons voisins, nous ne sommes pas toujours d''accord entre nous, mais nous d\\u00e9fendons :</p><ul><li data-block-key=\\\"2i4sq\\\">la parit\\u00e9 dans la gouvernance</li><li data-block-key=\\\"d2f7m\\\">des solutions collectives en bonne intelligence</li><li data-block-key=\\\"d9u2e\\\">l''expression libre</li><li data-block-key=\\\"ard5v\\\">l''exercice artistique</li><li data-block-key=\\\"1sv90\\\">l''apprentissage tout au long de la vie</li><li data-block-key=\\\"64jcl\\\">la valorisation de toutes formes de travail.</li></ul><p data-block-key=\\\"6q0us\\\">Aux Grands Voisins, nous vivons, travaillons, dormons, mangeons, nous soignons, achetons, pleurons, rions, et tout cela dans toutes les langues. L''inclusion et l''exclusion, toutes les deux, peuvent nous \\u00e9loigner de l''Autre qui est une chance.</p><p data-block-key=\\\"3khnu\\\">Nous r\\u00e9clamons des lieux de mixit\\u00e9 sociale qui permettent la valorisation et le partage des savoirs et des savoir-faire de toutes et de tous. Nous r\\u00e9clamons des lieux de mixit\\u00e9 sociale qui favorisent l''\\u00e9coute et les possibilit\\u00e9s de rencontre, conditions visant \\u00e0 l''abolition de la peur de l''autre. Nous r\\u00e9clamons des lieux de mixit\\u00e9 sociale qui luttent contre \\u00ab l''entre-soi \\u00bb, l''isolement et la r\\u00e9duction des individus \\u00e0 des cases.</p><p data-block-key=\\\"47p68\\\">Nous avons besoin de lieux et de temps pour faire pr\\u00e9c\\u00e9dent, pouvoir inventer d''autres formes de soci\\u00e9t\\u00e9 en harmonie avec les \\u00e9cosyst\\u00e8mes. Nous avons toutes et tous \\u00e0 donner et nous pouvons toutes et tous recevoir.</p>\", \"id\": \"a7fb2ebf-8e0d-401c-b11e-1929d826d8fa\"}]", "live": true, "path": "000100020004", "slug": "mission", "depth": 3, "owner": 1, "title": "Mission des Grands Voisin", "locale": 1, "locked": false, "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/mission-des-grands-voisin/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "Mission des Grands Voisins", "go_live_at": null, "draft_title": "Mission des Grands Voisin", "content_type": 25, "header_image": 48, "header_large": true, "header_darken": true, "live_revision": 65, "show_in_menus": true, "header_cta_link": null, "header_cta_text": null, "latest_revision": 65, "translation_key": "64b544ea-4a64-4a1c-8221-d1c40491f993", "contentpage_tags": [], "header_cta_label": null, "header_with_title": true, "last_published_at": "2024-05-06T03:21:08.643Z", "first_published_at": "2024-05-06T03:21:08.643Z", "header_color_class": "blue-ecume", "search_description": "Ce manifeste, imparfait, reflte les esprances, les souffrances et les incomprhensions de ceux qui l''ont insuffl. Les Grands Voisins sont ns  l''hpital Saint-Vincent-de-Paul dans le 14e arrondissement  Paris, entre Port royal et les Catacombes. Nous sommes issus des failles dans le bton et dans les politiques publiques. Nous sommes au service de l''accueil de tous et croyons aux ponts entre utopie et ralit.", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": "2024-05-06T03:21:08.623Z"}', NULL, '30', 1, 25, 1, 'Mission des Grands Voisin');
INSERT INTO public.wagtailcore_revision VALUES (40, '2024-05-05 17:59:33.844295+00', '{"pk": 19, "body": "[{\"type\": \"quote\", \"value\": {\"image\": 29, \"quote\": \"Comment \\u00eatre d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral, vraiment ? Peu apr\\u00e8s notre d\\u00e9claration, nous faillons peut-\\u00eatre d\\u00e9j\\u00e0. C''est un cadeau pour nous apprendre !\", \"author_name\": \"Madame la Ministre avec Ma\\u00ebl, Vice-Pr\\u00e9sident des Grands Voisins, \\u00e0 Inclusiv''Day 2023\", \"author_title\": \"\", \"color\": \"brown-opera\"}, \"id\": \"48c5d31b-88c6-442f-8046-f7c5bb180cb7\"}, {\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"o42mj\\\">Madame la Maire du 14e arrondissement conseille de laisser tomber l''argumentaire dit \\u00ab de responsabilit\\u00e9 de la marque \\u00bb Les Grands Voisins. J''avais utilis\\u00e9 l''argument le 25 mars 2023 sur l''ancien site Saint Vincent de Paul lorsque un cadre sup\\u00e9rieur de l''association Aurore m''a publiquement snob\\u00e9. Je n''\\u00e9tais pas en forme et l''article de blog \\u00e0 ce sujet \\u00e9tait aussi \\u00e9crit d''une posture d\\u00e9fensive.</p><p data-block-key=\\\"bjvv\\\">Je retiens de la suggestion une posture qui se base plus sur le pr\\u00e9sent et sur l''avenir !</p><p data-block-key=\\\"5gcf3\\\">Nous avons vot\\u00e9 une prise de position forte au 17e Conseil des Voisins pour nous d\\u00e9clarer organisme d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral. La d\\u00e9claration d''organisme d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral requiert trois conditions:</p><ol><li data-block-key=\\\"ffudt\\\">une gestion d\\u00e9sint\\u00e9ress\\u00e9e,</li><li data-block-key=\\\"b8cbc\\\">pas d''activit\\u00e9 lucrative,</li><li data-block-key=\\\"eif2t\\\">pas de fonctionnement au profit d''un cercle restreint.</li></ol><p data-block-key=\\\"focnb\\\">Pas plus tard que le 10 mai, d\\u00e9j\\u00e0 nous sommes test\\u00e9s sur les points 1 et 3 de par la visite de Madame la Ministre d\\u00e9l\\u00e9gu\\u00e9e au Handicap, Genevi\\u00e8ve Darrieussecq \\u00e0 Inclusiv''Day.</p>\", \"id\": \"68fe7661-6097-45c9-b54c-7c670b184e0f\"}, {\"type\": \"image\", \"value\": {\"title\": \"Ma\\u00ebl A\\u00efnine N\\u00e9ma Ch\\u00e9rif pour Les Grands Voisins avec Madame la Ministre D\\u00e9l\\u00e9gu\\u00e9e au Handicap, Genevi\\u00e8ve Darrieussecq\", \"heading_tag\": \"h3\", \"image\": 29, \"alt\": \"Ma\\u00ebl A\\u00efnine N\\u00e9ma Ch\\u00e9rif pour Les Grands Voisins avec Madame la Ministre D\\u00e9l\\u00e9gu\\u00e9e au Handicap, Genevi\\u00e8ve Darrieussecq\", \"caption\": \"Ma\\u00ebl A\\u00efnine N\\u00e9ma Ch\\u00e9rif pour Les Grands Voisins avec Madame la Ministre D\\u00e9l\\u00e9gu\\u00e9e au Handicap, Genevi\\u00e8ve Darrieussecq\", \"url\": \"\"}, \"id\": \"6c1bd7dc-4478-4ecf-8536-e4348a009777\"}, {\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"503h4\\\">Ma\\u00ebl, Vice Pr\\u00e9sident des Grands Voisins, et \\u00e0 Inclusiv''Day pour Les Grands Voisins a envoy\\u00e9 une lettre de sollicitation pour le compte de son association La Caravane CECAI un soutien, un financement, et un encouragement de financement de tiers.</p><p data-block-key=\\\"5o9uj\\\">Cette action m''incite \\u00e0 proposer urgemment un organe de contr\\u00f4le de notre adh\\u00e9rence \\u00e0 nos d\\u00e9clarations (Contrat pour le Web, Coop, Int\\u00e9r\\u00eat G\\u00e9n\\u00e9ral). Ma\\u00ebl pourrait \\u00eatre alors en erreur au regard de notre d\\u00e9claration d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral.</p><p data-block-key=\\\"blc0r\\\">La discussion \\u00e0 avoir pour Les Grands Voisins avec Mme. la Ministre D\\u00e9l\\u00e9gu\\u00e9e au Handicap tourne autour du <a href=\\\"https://blog.lesgrandsvoisins.com/manifeste\\\">Manifeste des Grands Voisins</a>. C''est pour cela que nous aurions besoin d''un comit\\u00e9 de contr\\u00f4le de notre d\\u00e9claration d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral. Pour comprendre l''erreur, imaginez que vous faite partie d''un organisme qui pr\\u00e9tend \\u00eatre d\\u00e9sint\\u00e9ress\\u00e9, puis vous apprenez que les dirigeants obtiennent des avantages pour des organismes tiers dans leur gestion de l''organisme.</p><p data-block-key=\\\"e1kvg\\\">Comme toute Grande Voisine et tout Grand Voisin, Ma\\u00ebl A.N.C. et la Caravane CECAI ont des projets louables, mais Les Grands Voisins ne peuvent pas soutenir directement ou indirectement aucune des initiatives. Soutenir une initiative porte automatiquement pr\\u00e9judice, \\u00e0 mon sens, \\u00e0 toutes les initiatives que nous ne soutenons pas. Malheureusement, cette tendance a une historique dans les Grands Voisins.</p>\", \"id\": \"ecf3d596-bcf9-4d59-b8dd-ccd1f6171916\"}, {\"type\": \"image\", \"value\": {\"title\": \"Une initiative \\u00ab SitesFaciles des Grands Voisins \\u00bb en avril 2019\", \"heading_tag\": \"h3\", \"image\": 30, \"alt\": \"Une initiative \\u00ab SitesFaciles des Grands Voisins \\u00bb en avril 2019\", \"caption\": \"Une initiative \\u00ab SitesFaciles des Grands Voisins \\u00bb en avril 2019\", \"url\": \"\"}, \"id\": \"e77fdcd9-a5bb-4b89-8c1d-36f064548769\"}, {\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"u2nv8\\\">En avril 2019, certains acteurs ont propos\\u00e9 un \\u00ab SitesFaciles des Grands Voisins \\u00bb dont on comptait princiaplement Les Petits D\\u00e9brouillard et Fr\\u00e9d\\u00e9ric G.. Ce projet avait, \\u00e0 mon sens, probablement d\\u00e9j\\u00e0 cette notion de profiter \\u00e0 un cercle restreint de personnes. Il avait pr\\u00e9vu explicitement un budget de 20.000 euros pour un financement de salaire \\u00e0 mi-temps pour Ma\\u00ebl ANC. Les montants de cette initiative l\\u00e0 \\u00e9taient petits.</p><p data-block-key=\\\"c65u5\\\">Notre d\\u00e9fi est grand. L''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral aurait besoin d''un comit\\u00e9 de contr\\u00f4le. Pour cela, la robustesse de la part de la contribution de Ma\\u00ebl ANC a apport\\u00e9 ses fruits.</p><p data-block-key=\\\"2mg4p\\\">Mon autre vice-pr\\u00e9sident me pose une question par rapport \\u00e0 un projet accessoire que nous avions avec Ma\\u00ebl \\u2013 le <a href=\\\"https://web.resdigita.com/?ref=blog.lesgrandsvoisins.com\\\">web fait main fait maison</a> \\u2013 qui \\u00e9tait con\\u00e7u pour financer notre pr\\u00e9sence \\u00e0 Inclusiv''Day. (A savoir que l''avance nous permettant \\u00e0 aller \\u00e0 Inclusv''Day venait de Ma\\u00ebl.) L''observation de Caroline \\u00e9tait que ce projet aurait pu enfreindre la condition 2 d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral. Chapeau Caroline !</p>\", \"id\": \"4246292d-7239-4a7f-9644-08e26324869c\"}, {\"type\": \"image\", \"value\": {\"title\": \"Mon erreur probable de proposer en kermesse les activit\\u00e9s b\\u00e9n\\u00e9voles pour le b\\u00e9n\\u00e9fice des Grands Voisins\", \"heading_tag\": \"h3\", \"image\": 31, \"alt\": \"Mon erreur probable de proposer en kermesse les activit\\u00e9s b\\u00e9n\\u00e9voles pour le b\\u00e9n\\u00e9fice des Grands Voisins\", \"caption\": \"Mon erreur probable de proposer en kermesse les activit\\u00e9s b\\u00e9n\\u00e9voles pour le b\\u00e9n\\u00e9fice des Grands Voisins\", \"url\": \"\"}, \"id\": \"8f1f3001-6ec5-4ee3-87a4-c1541d48cea1\"}, {\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"amgnp\\\">L''id\\u00e9e que j''avais pour le web fait main \\u00e9tait plus \\u00e0 l''image de kermesse d''\\u00e9cole. C''\\u00e9tait une activit\\u00e9 plus symbolique. Pourtant, la question de Caroline ouvre la question exactement o\\u00f9 elle doit aller ! (Et, effectivement, l''activit\\u00e9 de b\\u00e9n\\u00e9volat aurait besoin d''\\u00eatre scrut\\u00e9e.)</p><p data-block-key=\\\"73po8\\\">Il n''y auraient peut-\\u00eatre pas de probl\\u00e8me \\u00e0 mon sens que les acteurs se revendiquent des Grands Voisins dans leurs activit\\u00e9s. C''est un honneur pour Les Grands Voisins. Le probl\\u00e8me vient quand Les Grands Voisins utilise sa position pour promouvoir un acteur. La question est d''actualit\\u00e9 pour le sujet du budget participatif de Paris \\u00ab <a href=\\\"https://blog.lesgrandsvoisins.com/lancement-du-budget-participatif/\\\">le num\\u00e9rique cr\\u00e9atif des Grands Voisins</a> \\u00bb.</p><p data-block-key=\\\"7obtv\\\">Je dis que je suis laur\\u00e9at, car il s''agit d''une activit\\u00e9 de b\\u00e9n\\u00e9volat qui m''est personnelle sur le site Saint Vincent de Paul, \\u00e0 une \\u00e9glise depuis 2016 et au sein d''une resourcerie \\u00e0 Arcueil. C''est une chose que j''aime faire: \\u00e9quiper et animer des salles de sociabilit\\u00e9 num\\u00e9rique. On y aborde d''abord la sociabilit\\u00e9 et apr\\u00e8s le num\\u00e9rique afin de d\\u00e9stigmatiser le num\\u00e9rique. C''est vraiment splendide pour moi et pour beaucoup. Pourtant, effectivement, c''est mon kiffe, mon kiffe perso.</p>\", \"id\": \"f0595ab5-89af-44b0-afc5-552f5af5db1e\"}, {\"type\": \"image\", \"value\": {\"title\": \"La Mairie du 14e en avril 2022 lors de la consid\\u00e9ration du num\\u00e9rique cr\\u00e9atif des Grands Voisins\", \"heading_tag\": \"h3\", \"image\": 32, \"alt\": \"La Mairie du 14e en avril 2022 lors de la consid\\u00e9ration du num\\u00e9rique cr\\u00e9atif des Grands Voisins\", \"caption\": \"La Mairie du 14e en avril 2022 lors de la consid\\u00e9ration du num\\u00e9rique cr\\u00e9atif des Grands Voisins\", \"url\": \"\"}, \"id\": \"98a7f474-4a3d-4001-8607-df497475cf88\"}]", "date": "2024-05-05T17:45:00Z", "live": false, "path": "000100020003000D", "slug": "la-suite-des-grands-voisins", "depth": 4, "owner": 1, "title": "La Suite des Grands Voisins", "locale": 1, "locked": false, "authors": [], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/la-suite-des-grands-voisins/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "La Suite des Grands Voisins", "entry_tags": [], "go_live_at": null, "draft_title": "La Suite des Grands Voisins", "content_type": 71, "header_image": null, "header_large": false, "header_darken": false, "live_revision": null, "show_in_menus": false, "blog_categories": [], "header_cta_link": null, "header_cta_text": null, "latest_revision": 39, "translation_key": "100abfb8-1567-430c-8be0-f29103bb14e5", "entry_categories": [], "header_cta_label": null, "header_with_title": false, "last_published_at": null, "first_published_at": null, "header_color_class": null, "search_description": "Comment tre d''intrt gnral, vraiment ? Peu aprs notre dclaration, nous faillons peut-tre dj. C''est un cadeau pour nous apprendre !", "wagtail_admin_comments": [], "has_unpublished_changes": true, "latest_revision_created_at": "2024-05-05T17:58:18.930Z"}', NULL, '19', 1, 71, 1, 'La Suite des Grands Voisins');
