INSERT INTO public.sites_faciles_contentpage VALUES (117, '[{"id": "ecb4d132-119b-4148-8d3c-68dbfd7bd6ee", "type": "paragraph", "value": "<p data-block-key=\"ytcaw\">Participez  ces ateliers conviviaux dentraide numrique<br/>pour vous-mme ou pour votre organisation. Vous pouvez y avancer vos projets et vos savoir-faire aussi en aidant dautres avec les leurs.</p><p data-block-key=\"8etl9\">Tous niveaux, tous ges, tous besoins sont les bienvenus :<br/>+ des craintifs du numrique aux ingnieurs informaticiens ;<br/>+ des sans ordinateurs fixes aux quips du nec plus ultra ;<br/>+ des hbergs ou migrants aux propritaires ou entreprises du 14e ;<br/>+ des salaris ou non-salaris aux patrons du 14e ;<br/>+ des tudiants aux retraits ; etc.</p><p data-block-key=\"ahk8k\"><b>Le lundi de 16h  19h se tient un atelier dentraide numrique</b><br/><b> lAtelier de la Ressourcerie Crative</b> au 93 ave. du Gn. Leclerc et <b>en ligne </b> <a href=\"https://meet.lesgrandsvoisins.com/?ref=blog.lesgrandsvoisins.com\"><b>meet.lesgrandsvoisins.com</b></a>.<br/>En plus de vous-mmes et vos souhaits, vous pouvez amener votre quipement (ordinateur, tablette, smartphone, ), ventuellement un truc  manger et/ou  boire (sans alcool). Nous aurions des ordinateurs sur place et ventuellement dautres choses..</p><p data-block-key=\"catpb\">Programmation mensuelle  explorer : Fondation Valentin Hay 3 Rue Jacquier (quipement pour mal-voyants) ; La Table des Matires 51 Rue de l''Abb Carton ; peut-tre la Maison des rfugis, 50 Bd Jourdan ; Ailleurs ? (la salle est transportable)</p><p data-block-key=\"4m4ae\">Lancement bta  partir du lundi 7 aot, dfinitif  partir du lundi 4 septembre.<br/>Contact chris@lesgrandsvoisins.com au 07 81 81 18 11 pour toute question.<br/><a href=\"https://www.lesgrandsvosins.com/resdigita?ref=blog.lesgrandsvoisins.com\">https://www.lesgrandsvosins.com/resdigita</a></p>"}]', ,, 26, 'green-archipel', NULL, NULL, true, true, true, NULL));
