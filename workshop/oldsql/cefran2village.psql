INSERT INTO public.wagtailcore_revision VALUES (94, '2024-05-06 12:17:58.289435+00', '{"pk": 34, "body": "[{\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"yzz1i\\\">La DINUM a eu la gr\\u00e2ce de publier son moteur CMS pour numerique.gouv.fr sur github. Nous nous donnons comme mission de pouvoir rendre le b\\u00e9n\\u00e9fice de ce moteur aux acteurs priv\\u00e9s par le fait de remplacer la charte graphique de l''Etat par une charte g\\u00e9n\\u00e9rique, des chartes g\\u00e9n\\u00e9riques. Voici le lieu de notre travail:</p><p data-block-key=\\\"9fir4\\\"><a href=\\\"https://github.com/chris2fr/sites-faciles\\\">https://github.com/chris2fr/sites-faciles</a></p><p data-block-key=\\\"f6rl6\\\">Les choix prix par le DINUM, \\u00e0 partir du m\\u00eame socle technologique des Grands Voisins -- Wagtail, Django, Python -- sont structurants et rationnels. Le fait d''ouvrir ces d\\u00e9veloppements \\u00e0 un grand nombre nous permet de reverser \\u00e0 l''Etat nos contributions modestes. C''est un exemple de la puissance du dynamique des logiciels libres.</p><p data-block-key=\\\"8la9t\\\">Ce site en travaux est disponible sur <a href=\\\"https://facile.lesgrandsvoisins.com\\\">https://facile.lesgrandsvoisins.com</a>.</p><p data-block-key=\\\"fmm91\\\">Nous avons aussi essay\\u00e9 un site statique sur <a href=\\\"https://les.gv.coop\\\">https://les.gv.coop</a></p><p data-block-key=\\\"3djgc\\\">Le site actuel est sur <a href=\\\"https://www.lesgrandsvoisins.com\\\">https://www.lesgrandsvoisins.com</a></p>\", \"id\": \"87661a48-af93-4913-b933-f0afb4d285b7\"}, {\"type\": \"stepper\", \"value\": {\"title\": \"Mise en place du nouveau site web sous Sites-Faciles\", \"total\": 3, \"current\": 1, \"steps\": [{\"type\": \"step\", \"value\": {\"title\": \"Installation et tests\", \"detail\": \"Il s''agit d''installer le dispositif depuis sites-faciles, de faire des tests et d''ajuster le look-and-feel.\"}, \"id\": \"9a7f37fb-22e6-4878-80cd-8f8e31925334\"}, {\"type\": \"step\", \"value\": {\"title\": \"Validation et contenu\", \"detail\": \"Il s''agit d''organiser l''information et affiner la pr\\u00e9sentation avec une exp\\u00e9rience utilisateur convenable.\"}, \"id\": \"19234db7-2154-489c-9c57-d116199345c8\"}, {\"type\": \"step\", \"value\": {\"title\": \"Installation et mise en production\", \"detail\": \"Il s''agit de faire en sorte que le site web se charge rapidement et est conforme aux besoins de l''association et du Monde.\"}, \"id\": \"48947ec7-5ab0-48a9-8957-d813fc032715\"}]}, \"id\": \"1eea2c01-8d52-4398-8626-9f5298487fc0\"}, {\"type\": \"multicolumns\", \"value\": {\"bg_image\": null, \"bg_color_class\": \"\", \"bg_color\": \"\", \"title\": \"\", \"heading_tag\": \"h2\", \"columns\": [{\"type\": \"text\", \"value\": \"<h3 data-block-key=\\\"l1q4l\\\">Appropriation du dispositif de la DINUM</h3><p data-block-key=\\\"5d2ak\\\">Le dispositif <a href=\\\"https://sites-faciles.beta.numerique.gouv.fr/\\\">sites-faciles</a> de la DINUM est tr\\u00e8s \\u00e9troitement li\\u00e9 aux graphismes de l''Etat, mais a aussi des choix pratiques et communicatives pour des sites webs en France. Nous avons enregistr\\u00e9s <a href=\\\"https://github.com/numerique-gouv/sites-faciles/issues\\\">quelques items</a> avec la DINUM pour travailler sur ces sujets.</p>\", \"id\": \"bb0d1e3a-a505-44bd-ba0b-3663b6ab2b06\"}, {\"type\": \"text\", \"value\": \"<h3 data-block-key=\\\"00gq4\\\">Cr\\u00e9ation des contenus des GV</h3><p data-block-key=\\\"8f2nt\\\">Nous avons des contenus sur <a href=\\\"https://wagtail.lesgrandsvoisins.com\\\">wagtail.resdigita.com</a> et sur <a href=\\\"https://blog.lesgrandsvoisins.com\\\">blog.lesgrandsvoisins.com</a> \\u00e0 migrer sur ce nouveau dispositif.</p>\", \"id\": \"1d3be829-86b6-4bd6-93a8-64a11555df9f\"}]}, \"id\": \"70854f40-8c96-413f-b377-d9a1d1b0435a\"}]", "date": "2024-05-06T08:37:00Z", "live": true, "path": "000100020003000O", "slug": "sites-faciles", "depth": 4, "owner": 1, "title": "Refonte web avec Sites Faciles", "locale": 1, "locked": false, "authors": [1], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/sites-faciles/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "Refonte web avec avec Sites Faciles", "entry_tags": [{"pk": null, "tag": 34, "content_object": 34}, {"pk": null, "tag": 35, "content_object": 34}, {"pk": null, "tag": 36, "content_object": 34}], "go_live_at": null, "draft_title": "Refonte web avec Sites Faciles", "content_type": 71, "header_image": 53, "header_large": true, "header_darken": false, "live_revision": 92, "show_in_menus": true, "blog_categories": [2], "header_cta_link": null, "header_cta_text": null, "latest_revision": 93, "translation_key": "53775bc0-3da8-4a48-b082-d4b2cd50bd6b", "entry_categories": [{"pk": 26, "page": 34, "category": 2}], "header_cta_label": null, "header_with_title": false, "last_published_at": "2024-05-06T11:47:13.631Z", "first_published_at": "2024-05-06T08:45:06.274Z", "header_color_class": "grey", "search_description": "La DINUM a eu la grce de publier son moteur CMS pour numerique.gouv.fr sur github. Nous nous donnons comme mission []", "wagtail_admin_comments": [], "has_unpublished_changes": true, "latest_revision_created_at": "2024-05-06T12:16:23.161Z"}', NULL, '34', 1, 71, 1, 'Refonte web avec Sites Faciles');
INSERT INTO public.wagtailcore_revision VALUES (122, '2024-05-20 16:22:43.683788+00', '{"pk": 45, "body": "[{\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"805qw\\\">En g\\u00e9n\\u00e9ral, les Popup Expos des GV servent \\u00e0 inviter l''art hors les murs, c''est \\u00e0 dire dans des lieux o\\u00f9 on ne l''attend pas pour :</p><ul><li data-block-key=\\\"divs7\\\">Favoriser une vie de quartier selon les principes des Grands Voisins</li><li data-block-key=\\\"dhu08\\\">Inviter l\\u2019art dans le quotidien du grand public en cassant les id\\u00e9es re\\u00e7ues que l\\u2019art est r\\u00e9serv\\u00e9 \\u00e0 une partie de la population</li><li data-block-key=\\\"8qh4r\\\">Cr\\u00e9er une synergie entre le commerce et la culture</li><li data-block-key=\\\"6n85i\\\">Mettre en lumi\\u00e8re les artistes locaux et les aider \\u00e0 gagner en visibilit\\u00e9</li><li data-block-key=\\\"bg48g\\\">Dynamiser l\\u2019\\u00e9conomie locale</li><li data-block-key=\\\"5qlf5\\\">Valoriser notre patrimoine et culture fran\\u00e7ais et nos artistes</li></ul><p data-block-key=\\\"dbc0q\\\">Et favoriser ainsi l''acc\\u00e8s \\u00e0 la culture, l''apaisement, la d\\u00e9tente, l''\\u00e9tonnement...</p><p data-block-key=\\\"6sfu5\\\"><i>Animer l''IRMA Valenton en organisant une exposition peut avoir de nombreux avantages, \\u00e0 la fois pour l''h\\u00f4pital et les patients.</i></p><h2 data-block-key=\\\"1og05\\\">Les Popup expos pour les patients</h2><p data-block-key=\\\"4fl3e\\\">Nous proposons d\\u2019inscrire les Popup expos dans le parcours patient, participant \\u00e0 d\\u00e9centrer la douleur physique et psychologique des patients. En leur donnant une autre vision de leur nouveau quotidien, nous souhaitons participer \\u00e0 favoriser leur acceptation de leur nouveau corps gr\\u00e2ce \\u00e0 un environnement toujours plus humain et agr\\u00e9able.</p><ul><li data-block-key=\\\"5cq54\\\">Distraction positive :<br/>Offrir aux patients une \\u00e9vasion au cours de leur s\\u00e9jour m\\u00e9dical, contribuant \\u00e0 r\\u00e9duire le stress et l\\u2019anxi\\u00e9t\\u00e9 associ\\u00e9s \\u00e0 l\\u2019hospitalisation</li><li data-block-key=\\\"fr8cd\\\">Stimulation mentale :<br/>Stimuler l\\u2019esprit des patients en les aidant \\u00e0 rester mentalement actifs et engag\\u00e9s</li><li data-block-key=\\\"bm8l6\\\">Education :<br/>Le choix des sujets des expositions peut aider les patients \\u00e0 mieux comprendre leur condition et s\\u2019approprier mieux leur nouveau corps</li><li data-block-key=\\\"1d8bc\\\">Cr\\u00e9ativit\\u00e9 et expression :<br/>Favoriser l\\u2019expression cr\\u00e9ative et \\u00e9motionnelle des patients</li><li data-block-key=\\\"fojgu\\\">Interaction sociale :<br/>Cr\\u00e9er un environnement propice \\u00e0 la rencontre et \\u00e0 l\\u2019\\u00e9change entre les patients, les familles, le personnel et contribuer ainsi \\u00e0 r\\u00e9duire l\\u2019isolement</li></ul><h2 data-block-key=\\\"a9i9g\\\">Les Popup expos pour l\\u2019IRMA</h2><p data-block-key=\\\"4vhk4\\\">Nous proposons d\\u2019inscrire les Popup expos dans le projet de l''h\\u00f4pital</p><ul><li data-block-key=\\\"3ec59\\\">Promotion de la sant\\u00e9 et sensibilisation :<br/>Les expos peuvent \\u00eatre con\\u00e7ues pour sensibiliser le public sur les questions de sant\\u00e9 sp\\u00e9cifiques, telles que la pr\\u00e9vention des maladies, promotion d\\u2019habitudes et mode de vie sains ou encore la sensibilisation \\u00e0 leurs conditions m\\u00e9dicales</li><li data-block-key=\\\"6drii\\\">Renforcement de l\\u2019image de l''h\\u00f4pital :<br/>Organiser des \\u00e9v\\u00e9nements culturels et artistiques peut participer \\u00e0 l\\u2019am\\u00e9lioration de l\\u2019image de l''h\\u00f4pital, soucieux non seulement du traitement m\\u00e9dical mais aussi du bien-\\u00eatre de la communaut\\u00e9</li><li data-block-key=\\\"3j21e\\\">Engagement :<br/>Opportunit\\u00e9 de s''impliquer et de s\\u2019inscrire davantage dans l\\u2019environnement local</li><li data-block-key=\\\"1ug1s\\\">Cr\\u00e9ation de lien :<br/>Les expositions peuvent attirer un large \\u00e9ventail de participants, y compris des experts m\\u00e9dicaux, des artistes locaux et d''autres professionnels. Cela peut favoriser la cr\\u00e9ation de partenariats b\\u00e9n\\u00e9fiques</li><li data-block-key=\\\"2hdso\\\">Diversification des activit\\u00e9s de l''h\\u00f4pital :<br/>En offrant des activit\\u00e9s culturelles et artistiques en plus des soins m\\u00e9dicaux et param\\u00e9dicaux, l''h\\u00f4pital peut diversifier ses services et devenir un lieu plus inclusif.</li></ul>\", \"id\": \"87907458-90d4-488d-9104-e687407ca69e\"}, {\"type\": \"paragraph\", \"value\": \"<h2 data-block-key=\\\"805qw\\\">Nos actions concr\\u00e8tes</h2><p data-block-key=\\\"coceh\\\">Nos actions</p><ul><li data-block-key=\\\"1b2em\\\">Recherches d''artistes</li><li data-block-key=\\\"6b2q9\\\">Gestion des candidatures</li><li data-block-key=\\\"d5cgl\\\">Proposition d''un projet articul\\u00e9 autour des artistes et leur cr\\u00e9ativit\\u00e9 et le lieu d''exposition</li><li data-block-key=\\\"c5vdg\\\">Logistique</li><li data-block-key=\\\"2tk9g\\\">Accrochage/D\\u00e9crochage</li><li data-block-key=\\\"9o9uo\\\">Organisation de rencontres avec les artistes</li></ul><h2 data-block-key=\\\"29r09\\\">M\\u00e9tamorphose Artistique : \\u00c9motions et transformations</h2><p data-block-key=\\\"a5abi\\\">Cette exposition unique offre une plong\\u00e9e artistique au c\\u0153ur de l''exp\\u00e9rience humaine, explorant la r\\u00e9silience, l''espoir et la reconstruction. Chaque \\u0153uvre expos\\u00e9e est une c\\u00e9l\\u00e9bration de la force int\\u00e9rieure et de la beaut\\u00e9 qui \\u00e9merge de la transformation apr\\u00e8s des moments difficiles v\\u00e9cus par les artistes et qui racontent des histoires de courage, d''adaptation et de renouveau.</p><p data-block-key=\\\"4g6ql\\\">Le choix du sujet repose sur la conviction que l''art peut \\u00eatre un catalyseur puissant pour l''expression \\u00e9motionnelle, la compr\\u00e9hension mutuelle et la transformation.</p><p data-block-key=\\\"aacii\\\">L''exposition vise \\u00e0 humaniser l''exp\\u00e9rience de l''amputation en mettant en avant les r\\u00e9cits individuels \\u00e0 travers des \\u0153uvres d''art visuelles, sculpturales, et d''autres formes artistiques. En montrant la force, la vuln\\u00e9rabilit\\u00e9 et la beaut\\u00e9 dans la diversit\\u00e9 des exp\\u00e9riences humaines, nous esp\\u00e9rons cr\\u00e9er un espace o\\u00f9 patients, familles et personnel m\\u00e9dical peuvent se connecter et trouver r\\u00e9confort.</p><h2 data-block-key=\\\"2nubp\\\">D\\u00e9tails</h2><h3 data-block-key=\\\"b4d2k\\\">\\u0152uvres Multidisciplinaires</h3><p data-block-key=\\\"9doak\\\">Les visiteurs auront l''occasion d''explorer une vari\\u00e9t\\u00e9 d''expressions artistiques, y compris la peinture, la sculpture et la photographie</p><h3 data-block-key=\\\"f31lv\\\">M\\u00e9tamorphose Artistique</h3><p data-block-key=\\\"bu8ap\\\">\\u00c9motions et Reconstructions\\\" c\\u00e9l\\u00e8bre la cr\\u00e9ativit\\u00e9 en tant que catalyseur de gu\\u00e9rison et d''unit\\u00e9. Nous invitons la communaut\\u00e9 \\u00e0 se joindre \\u00e0 cette exploration visuelle de la r\\u00e9silience humaine, \\u00e0 travers les yeux d''artistes exceptionnels qui transforment l''exp\\u00e9rience de l''amputation en une \\u0153uvre d''art vivante.</p><h2 data-block-key=\\\"a47gv\\\">Le champ des possibles</h2><h3 data-block-key=\\\"csar3\\\">Ateliers de cr\\u00e9ation artistique</h3><p data-block-key=\\\"50b66\\\">Organiser des ateliers de peinture, de sculpture, de dessin ou d''autres formes d''expression artistique pour les patients. Cela peut \\u00eatre particuli\\u00e8rement b\\u00e9n\\u00e9fique pour ceux qui sont en phase de r\\u00e9tablissement ou qui font face \\u00e0 des probl\\u00e8mes de sant\\u00e9 mentale.</p><h3 data-block-key=\\\"l317\\\">Mur d''art communautaire</h3><p data-block-key=\\\"b5am9\\\">Cr\\u00e9er un espace o\\u00f9 les patients, les visiteurs et le personnel peuvent contribuer \\u00e0 une \\u0153uvre d''art collective. Cela favorise un sentiment d''appartenance et de communaut\\u00e9.</p><h3 data-block-key=\\\"83l2i\\\">Expositions artistiques</h3><p data-block-key=\\\"23qh2\\\">Organiser des expositions d''\\u0153uvres cr\\u00e9\\u00e9es par les patients. Cela peut aider \\u00e0 sensibiliser \\u00e0 certaines conditions m\\u00e9dicales et \\u00e0 d\\u00e9stigmatiser les maladies mentales.</p><h3 data-block-key=\\\"c01hk\\\">Ateliers de gestion du stress</h3><p data-block-key=\\\"9qfas\\\">Offrir des s\\u00e9ances de cr\\u00e9ation artistique pour le personnel m\\u00e9dical afin de les aider \\u00e0 g\\u00e9rer le stress li\\u00e9 \\u00e0 leur travail.</p><h3 data-block-key=\\\"9iu73\\\">D\\u00e9coration artistique</h3><p data-block-key=\\\"dk473\\\">Embellir les espaces hospitaliers avec des \\u0153uvres d''art, des peintures murales, des sculptures, contribuant ainsi \\u00e0 cr\\u00e9er un environnement plus accueillant et apaisant.</p><h3 data-block-key=\\\"9b1tb\\\">Jardins th\\u00e9rapeutiques</h3><p data-block-key=\\\"9fm3h\\\">Am\\u00e9nager des espaces ext\\u00e9rieurs avec des \\u00e9l\\u00e9ments artistiques, favorisant la relaxation et la gu\\u00e9rison.</p><h3 data-block-key=\\\"570a4\\\">Collaborations artistiques</h3><p data-block-key=\\\"8m8pk\\\">\\u00c9tablir des partenariats avec des artistes locaux pour des r\\u00e9sidences artistiques ou des interventions sp\\u00e9cifiques \\u00e0 l''h\\u00f4pital.</p><h3 data-block-key=\\\"18q5l\\\">Int\\u00e9gration du spectacle vivant</h3><p data-block-key=\\\"9f453\\\">Offrir un moment d\\u2019\\u00e9vasion et de divertissement</p>\", \"id\": \"96822e24-d8ec-48fa-922c-d2a42f8e7a3d\"}]", "live": true, "path": "0001000200060002", "slug": "irma-valenton", "depth": 4, "owner": 1, "title": "Hpital aussi Gallrie  l''IRMA Valenton", "locale": 1, "locked": false, "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/arts-plastiques/irma-valenton/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "", "go_live_at": null, "draft_title": "Hpital aussi Gallrie  l''IRMA Valenton", "content_type": 25, "header_image": null, "header_large": false, "header_darken": false, "live_revision": null, "show_in_menus": true, "header_cta_link": null, "header_cta_text": null, "latest_revision": null, "translation_key": "2f74a6a3-3e41-42f7-9d55-4a8b5d20b394", "contentpage_tags": [], "header_cta_label": null, "header_with_title": false, "last_published_at": null, "first_published_at": null, "header_color_class": null, "search_description": "En gnral, les Popup Expos des GV servent  inviter l''art hors les murs, c''est  dire dans des lieux []", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": null}', NULL, '45', 1, 25, 1, 'Hpital aussi Gallrie  l''IRMA Valenton');
INSERT INTO public.wagtailcore_revision VALUES (171, '2024-05-27 08:51:34.286732+00', '{"pk": 51, "body": "[{\"type\": \"multicolumns\", \"value\": {\"bg_image\": null, \"bg_color_class\": \"\", \"bg_color\": \"\", \"title\": \"\", \"heading_tag\": \"h2\", \"columns\": [{\"type\": \"text\", \"value\": \"<h3 data-block-key=\\\"wopaa\\\">22nd Conseil of Les Grands Voisins</h3><p data-block-key=\\\"9sn6r\\\"></p><embed alt=\\\"flyer-20240506-conseildesgv22\\\" embedtype=\\\"image\\\" format=\\\"left\\\" id=\\\"54\\\"/><p data-block-key=\\\"4jn74\\\"></p><p data-block-key=\\\"f38i5\\\">Nous tiendrions le 22e Conseil des Voisins sur trois rencontres :</p><ul><li data-block-key=\\\"sqrq\\\">Lundi le 10 juin de 18h \\u00e0 19h30</li><li data-block-key=\\\"5sba\\\">Jeudi le 13 juin de 19h \\u00e0 20h30</li><li data-block-key=\\\"fabkv\\\">Samedi le le 15 juin de 10h30 \\u00e0 midi</li></ul><p data-block-key=\\\"a6q9u\\\">Ordre du jour :</p><ol><li data-block-key=\\\"43o40\\\">Num\\u00e9rique</li><li data-block-key=\\\"oioh\\\">\\u00c9ventuellement Civisme</li><li data-block-key=\\\"7l6q8\\\">Communication</li></ol>\", \"id\": \"81d80008-6286-46b4-800e-edf8d4fce080\"}, {\"type\": \"text\", \"value\": \"<h3 data-block-key=\\\"avd09\\\"><b>Permanence le lundi de midi \\u00e0 17h</b></h3><p data-block-key=\\\"e0hvf\\\"></p><embed alt=\\\"Abaker-Atelier\\\" embedtype=\\\"image\\\" format=\\\"left\\\" id=\\\"52\\\"/><p data-block-key=\\\"5c5gg\\\"></p><p data-block-key=\\\"dqj9a\\\">Venez \\u00e0 notre permanence <b>le lundi de 12h \\u00e0 17h</b><br/>en personne \\u00e0 <b>93 ave. Du G\\u00e9n. Leclerc</b> 75014 Paris (M\\u00e9tro Al\\u00e9sia, Atelier de la Ressourcerie, <i>7e boutique sur la gauche</i>) ou <a href=\\\"https://meet.lesg/\\\">meet.lesgrandsvoisins.com pour participer en hybride</a>.</p>\", \"id\": \"460c495c-476c-43f1-bb69-aff7bf1509ea\"}, {\"type\": \"text\", \"value\": \"<h3 data-block-key=\\\"avd09\\\"><a href=\\\"https://www.lesgrandsvoisins.fr/numerique/numerique-creatif/sociabilite-numerique/\\\"><b>Sociabilit\\u00e9 num\\u00e9rique le lundi de 17h \\u00e0 21h30</b></a></h3><embed alt=\\\"numasso\\\" embedtype=\\\"image\\\" format=\\\"right\\\" id=\\\"22\\\"/><p data-block-key=\\\"cda90\\\"><b>Le lundi de 17h \\u00e0 21h30</b> se tient un atelier d\\u2019entraide num\\u00e9rique \\u00e0 l\\u2019Atelier au <b>93 ave. du G\\u00e9n. Leclerc 75014 PARIS</b> \\u00e0 l\\u2019int\\u00e9rieur du <i>centre commercial 7e boutique sur la gauche</i></p>\", \"id\": \"6fc069f3-372a-447a-8d33-fd26389bdac8\"}]}, \"id\": \"c58f3abe-971d-41a7-ad49-f547da49dd04\"}, {\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"q7an3\\\"><a href=\\\"https://list.lesgrandsvoisins.com/subscription/form\\\"><b><i>Pensez \\u00e0 vous inscrire sur notre liste d''infos !</i></b></a></p><p data-block-key=\\\"evpog\\\">Nous sommes une association d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral agissant de mani\\u00e8re contr\\u00f4l\\u00e9e en subsidiarit\\u00e9 ouverte pour toute organisation et tout individu qui en fait la d\\u00e9cision pour soi-m\\u00eame principalement en ces trois domaines <a href=\\\"https://www.lesgrandsvoisins.fr/communautes/\\\">de Discussions</a>, <a href=\\\"https://www.lesgrandsvoisins.fr/arts/\\\">d''Arts plastiques</a> et <a href=\\\"https://www.lesgrandsvoisins.fr/numerique/\\\">de Num\\u00e9rique</a> selon <a href=\\\"https://www.lesgrandsvoisins.fr/mission/\\\">notre d\\u00e9claration de mission</a>.</p><p data-block-key=\\\"bj8u6\\\"><a href=\\\"https://www.lesgrandsvoisins.fr/communautes/\\\"><b>Discussions</b></a> :</p><ul><li data-block-key=\\\"61obe\\\">Nous proposons des conditions encourageant la compr\\u00e9hension entre individus et organisations de perspectives diff\\u00e9rentes.</li><li data-block-key=\\\"9oi9e\\\">Nous ouvrons notre gouvernance pour des votes en Conseils des Voisins.</li></ul><p data-block-key=\\\"65va7\\\"><a href=\\\"https://www.lesgrandsvoisins.fr/arts/\\\"><b>Arts plastiques</b></a> :</p><ul><li data-block-key=\\\"a2qag\\\">Nous encourageons l\\u2019utilisation hybrides d\\u2019espace (commerces, h\\u00f4pitaux, caf\\u00e9s, etc.) aussi en gall\\u00e9ries locales.</li><li data-block-key=\\\"2ek3r\\\">Nous accompagnons des institutions pour trouver une juste place pour les arts plastiques dans leurs locaux.</li></ul><p data-block-key=\\\"91unk\\\"><a href=\\\"https://www.lesgrandsvoisins.fr/numerique/\\\"><b>Num\\u00e9rique</b></a> :</p><ul><li data-block-key=\\\"afesu\\\">Nous maintenons une constellation de serveurs et de services internet -- tableau de bord, cr\\u00e9ation et gestion de compte, courriel, rencontres hybrides pr\\u00e9sence-distance, listes de diffusion num\\u00e9riques, blogs, serveurs webs, gestionnaire de mot de passe, enregistrement dans le nuage, forums de discussion, gestion de la connaissance, \\u00e9dition markdown, wikis, \\u00e9dition bueautique, etc. -- pour, par, et de nous-m\\u00eames et d''autres.</li><li data-block-key=\\\"ej6ca\\\">Nous mettons \\u00e0 disposition des talents et des mat\\u00e9riaux dans des salles de sociabilit\\u00e9 num\\u00e9rique et dans des syst\\u00e8mes de pr\\u00eats et \\u00e9changes de mat\\u00e9riels \\u00e0 l''attention d''individus et d''organisations de tous niveaux.</li></ul><p data-block-key=\\\"apjqo\\\">Voici quelques entit\\u00e9s Grandes Voisines qui soutiennent notre d\\u00e9marche: A Good Solution, Banya Tour, Robert Baldridge, BMCP, Breast Freedom, Le Bureau du Cr\\u00e9ateur, Le Chiffon, Les Jardins du Num\\u00e9rique, Olivier Duhec, Inno Cube, Caroline Lhomme, Francesco du 59 Rivoli, Amadou Gaye, Informatruck, Inolib, KaceKode, Ada Matus, L\\u2019Odys\\u00e9e, La Ressourcerie Cr\\u00e9ative, Henry Roy, Peter Vaeth, Waka Conseil, Weezem, et peut-\\u00eatre des v\\u00f4tres \\u2026</p><p data-block-key=\\\"at7dp\\\">Pour plus d\\u2019informations, contactez-nous\\u00a0: chris@lesgrandsvoisins.com +33 7 81 811 811</p><p data-block-key=\\\"4coel\\\"></p>\", \"id\": \"d3e4fb2e-7b28-4da0-99d3-09c2868a013f\"}, {\"type\": \"video\", \"value\": {\"title\": \"Les Grands Voisins, la cit\\u00e9 r\\u00eav\\u00e9e\", \"caption\": \"c 3, 2020 R\\u00e9alisation : Bastien Simon  Pitch : Ma\\u00ebl, artiste sans papier, Adrien, luthier musicien et d''autres r\\u00e9sidents venus de tous horizons, s''organisent pour donner naissance \\u00e0 une utopie moderne en plein coeur de Paris, un village solidaire de pr\\u00e8s de 2000 personnes : Les Grands Voisins. \\u00c0 travers leurs trajectoires et celles des membres fondateurs du lieu, le film interroge notre d\\u00e9sir et notre capacit\\u00e9 \\u00e0 inventer d\\u2019autres mani\\u00e8res de vivre ensemble. Que retiendrons-nous de cette exp\\u00e9rience collective ? Pourra-t-elle perdurer, essaimer, r\\u00e9sonner ailleurs ?\", \"url\": \"https://www.youtube.com/embed/qZqErGfNF_o?feature=oembed\"}, \"id\": \"f53195d5-0de5-479d-aef1-6151fbd266e0\"}, {\"type\": \"accordions\", \"value\": [{\"type\": \"title\", \"value\": \"Les Grands Voisins\", \"id\": \"6c6ec6eb-93af-4f4a-97e4-2f91d2498c57\"}, {\"type\": \"accordion\", \"value\": {\"title\": \"Sites Webs des Grands Voisins\", \"content\": \"<p data-block-key=\\\"h0w28\\\"><a href=\\\"https://www.lesgrandsvoisins.fr\\\">LesGrandsVoisins.fr</a> : Le site public principal des Grands Voisins</p><p data-block-key=\\\"6dm50\\\"><a href=\\\"https://www.resdigita.com\\\">ResDigita.com</a> : Le num\\u00e9rique des Grands Voisins se nomme ResDigita.</p><p data-block-key=\\\"3ntu0\\\"><a href=\\\"https://blog.lesgrandsvoisins.com\\\">Blog.LesGrandsVoisins.com</a> : Notre blog (accessible depuis le site principal)</p>\"}, \"id\": \"d2cecf58-044d-441c-849d-2d98d2085f7f\"}, {\"type\": \"accordion\", \"value\": {\"title\": \"M\\u00e9dias Sociaux des Grands Voisins\", \"content\": \"<p data-block-key=\\\"h0w28\\\"><a href=\\\"https://www.instagram.com/les_grands_voisins/\\\">Instagram@les_grands_voisins</a></p><p data-block-key=\\\"eseqg\\\">Facebook</p><p data-block-k