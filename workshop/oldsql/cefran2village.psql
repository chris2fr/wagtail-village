INSERT INTO public.sites_faciles_contentpage VALUES (107, '[{"id": "66ff0747-1ccf-4761-8adc-4a805dd4e883", "type": "imageandtext", "value": {"link": {"page": null, "text": "", "external_url": ""}, "page": null, "text": "<p data-block-key=\"8vslz\"><i>Changeons le monde ensembles.</i></p><p data-block-key=\"cbep\">Nous proposons notre 21e Conseil des Voisins le 28 avril  10h dans le 14e arrondissement  Paris et en ligne sur <a href=\"https://meet.lesgrandsvoisins.com/?ref=blog.lesgrandsvoisins.com\">meet.lesgrandsvoisins.com</a></p><p data-block-key=\"7c42l\">Chez Papa Montparnasse Vavin (RER B Port Royal ou Metro 4 Vavin)<br/>138 Boulevard du Montparnasse, 75014 Paris</p><p data-block-key=\"3h4r2\">(<a href=\"https://maps.app.goo.gl/9F1w2Fam8goZ3ASx9?ref=blog.lesgrandsvoisins.com\">lien Maps</a>)</p><p data-block-key=\"aqqh1\">Ordre du jour :</p><ol><li data-block-key=\"d29qa\">Comptes annuels (faibles)</li><li data-block-key=\"9oc8p\">Activits : arts plastiques, numrique, civisme</li><li data-block-key=\"4fef0\">Communication</li></ol><p data-block-key=\"6p2dh\">Nous parlerions des sujets prcis en lien avec la mmoire de Valentin FRANCY, le numrique cratif des Grands Voisins (Budget Participatif de Paris), efforts pour rendre des hpitaux hybrides aussi en galeries d''art.</p><p data-block-key=\"ahns\"><a href=\"https://www.instagram.com/les_grands_voisins/?utm_source=ig_embed&amp;ig_rid=f6153464-ce6a-41dd-b392-5fc008623cfa\">https://www.instagram.com/les_grands_voisins/?utm_source=ig_embed&amp;ig_rid=f6153464-ce6a-41dd-b392-5fc008623cfa</a></p><p data-block-key=\"265ce\"></p>", "image": 10, "link_url": "", "image_side": "right", "link_label": "", "image_ratio": "3"}}]', ,, 10, 'blue-france', NULL, NULL, true, true, true, NULL));
