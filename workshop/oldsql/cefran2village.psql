INSERT INTO public.wagtail_localize_string VALUES (15, 'caedadd4-8baf-51fb-9050-26c89ea1225b', 'Venez  notre permanence <b>le lundi de 12h  17h</b><br/>en personne  <b>93 ave. Du Gn. Leclerc</b> 75014 Paris (Mtro Alsia, Atelier de la Ressourcerie, <i>7e boutique sur la gauche</i>) ou <a id="a1">meet.lesgrandsvoisins.com pour participer en hybride</a>.', 1);
INSERT INTO public.wagtail_localize_string VALUES (16, 'efa7ba43-3d0a-56c7-b110-8a7c460edc7b', '<a id="a1"><b>Sociabilit numrique le lundi de 17h  21h30</b></a>', 1);
INSERT INTO public.wagtail_localize_string VALUES (17, 'eb7d9764-5632-55f2-bd2e-39b94f4d5ff1', '<b>Le lundi de 17h  21h30</b> se tient un atelier dentraide numrique  lAtelier au <b>93 ave. du Gn. Leclerc 75014 PARIS</b>  lintrieur du <i>centre commercial 7e boutique sur la gauche</i>', 1);
INSERT INTO public.wagtail_localize_string VALUES (18, '5847d25a-f7e1-52f2-8768-ad66d06996e5', '<a id="a1"><b><i>Pensez  vous inscrire sur notre liste d''infos !</i></b></a>', 1);
INSERT INTO public.wagtail_localize_string VALUES (19, 'bf8c4046-6f0d-5d63-802e-afba91d45ed4', 'Nous sommes une association d''intrt gnral agissant de manire contrle en subsidiarit ouverte pour toute organisation et tout individu qui en fait la dcision pour soi-mme principalement en ces trois domaines <a id="a1">de Discussions</a>, <a id="a2">d''Arts plastiques</a> et <a id="a3">de Numrique</a> selon <a id="a4">notre dclaration de mission</a>.', 1);
INSERT INTO public.wagtail_localize_string VALUES (20, '46656e0f-b62c-5f04-9f21-1365356624f2', '<a id="a1"><b>Discussions</b></a> :', 1);
INSERT INTO public.wagtail_localize_string VALUES (21, '7f385e3e-823e-5e3c-89c9-be7d476a05e4', 'Nous proposons des conditions encourageant la comprhension entre individus et organisations de perspectives diffrentes.', 1);
INSERT INTO public.wagtail_localize_string VALUES (22, '4d732007-d59d-5e0e-b30a-f7a65b02b85e', 'Nous ouvrons notre gouvernance pour des votes en Conseils des Voisins.', 1);
INSERT INTO public.wagtail_localize_string VALUES (23, 'b9e331ee-8ba7-52c9-9ded-2b6b7d3c7e51', '<a id="a1"><b>Arts plastiques</b></a> :', 1);
INSERT INTO public.wagtail_localize_string VALUES (24, '12eaef79-6b95-544d-be2f-b59a7fc7d0d3', 'Nous encourageons lutilisation hybrides despace (commerces, hpitaux, cafs, etc.) aussi en gallries locales.', 1);
INSERT INTO public.wagtail_localize_string VALUES (25, 'f518273a-d81f-5a17-812d-f8e3a6f86851', 'Nous accompagnons des institutions pour trouver une juste place pour les arts plastiques dans leurs locaux.', 1);
INSERT INTO public.wagtail_localize_string VALUES (26, '15689669-825f-5455-8e13-6d3f77fc3dc7', '<a id="a1"><b>Numrique</b></a> :', 1);
INSERT INTO public.wagtail_localize_string VALUES (27, '4c04c170-dddc-5a70-a366-f0df9f37ba84', 'Nous maintenons une constellation de serveurs et de services internet -- tableau de bord, cration et gestion de compte, courriel, rencontres hybrides prsence-distance, listes de diffusion numriques, blogs, serveurs webs, gestionnaire de mot de passe, enregistrement dans le nuage, forums de discussion, gestion de la connaissance, dition markdown, wikis, dition bueautique, etc. -- pour, par, et de nous-mmes et d''autres.', 1);
INSERT INTO public.wagtail_localize_string VALUES (28, '78438c49-61c4-553e-bff1-87cb7418b982', 'Nous mettons  disposition des talents et des matriaux dans des salles de sociabilit numrique et dans des systmes de prts et changes de matriels  l''attention d''individus et d''organisations de tous niveaux.', 1);
INSERT INTO public.wagtail_localize_string VALUES (29, '808637d0-3624-5eb5-a7d9-9313823f62be', 'Voici quelques entits Grandes Voisines qui soutiennent notre dmarche: A Good Solution, Banya Tour, Robert Baldridge, BMCP, Breast Freedom, Le Bureau du Crateur, Le Chiffon, Les Jardins du Numrique, Olivier Duhec, Inno Cube, Caroline Lhomme, Francesco du 59 Rivoli, Amadou Gaye, Informatruck, Inolib, KaceKode, Ada Matus, LOdyse, La Ressourcerie Crative, Henry Roy, Peter Vaeth, Waka Conseil, Weezem, et peut-tre des vtres ', 1);
INSERT INTO public.wagtail_localize_string VALUES (30, '3294abc4-3005-53c6-8721-b21822e3b23d', 'Pour plus dinformations, contactez-nous: chris@lesgrandsvoisins.com +33 7 81 811 811', 1);
INSERT INTO public.wagtail_localize_string VALUES (31, '643f4e32-ae23-5421-9bd5-cae3bcd2141a', 'Les Grands Voisins, la cit rve', 1);
INSERT INTO public.wagtail_localize_string VALUES (32, '5bb4cab3-d1db-53a7-85d8-d9b10bc4f290', 'c 3, 2020 Ralisation : Bastien Simon  Pitch : Mal, artiste sans papier, Adrien, luthier musicien et d''autres rsidents venus de tous horizons, s''organisent pour donner naissance  une utopie moderne en plein coeur de Paris, un village solidaire de prs de 2000 personnes : Les Grands Voisins.  travers leurs trajectoires et celles des membres fondateurs du lieu, le film interroge notre dsir et notre capacit  inventer dautres manires de vivre ensemble. Que retiendrons-nous de cette exprience collective ? Pourra-t-elle perdurer, essaimer, rsonner ailleurs ?', 1);
