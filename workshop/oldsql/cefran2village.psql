INSERT INTO public.wagtail_localize_string VALUES (79, '713a68b3-c8ae-574d-80b7-00525be7e87a', 'Les Grands Voisins est une association dirige par tout individu qui veut bien pour l''entraide et le partage de biens communs entre artistes, artisans, professionnels, associations, institutions et entreprises. Nous nous concentrons sur les arts plastiques, le numrique et le civisme.', 1);
