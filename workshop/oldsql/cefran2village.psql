INSERT INTO public.sites_faciles_contentpage VALUES (127, '[{"id": "59753479-bd84-413b-bd65-ccfb0160a5b6", "type": "paragraph", "value": "<p data-block-key=\"1qwaf\">Le 10 mai 2023  Paris La Dfense Arena, venez nous rendre visite ainsi que d''autres acteurs autour du handicap et de la ralisation. Sur notre stand, nous aurions de l''art, le livre de Caroline et des dmonstrations de nos axes digital et artistiques. L''organisateur parle dun riche programme ditorial (management &amp; stratgie RSE, emploi inclusif, achats responsables, innovations sociales, accessibilit numrique, actualits handicap, ateliers de sensibilisation) des confrences thmatiques, des ateliers de formation, des sances de networking et des animations immersives.</p><p data-block-key=\"2dc2f\">Inscription ici pour Les Grands Voisins et sur le site de l''organisateur respectivement:</p><p data-block-key=\"f72ld\"><a href=\"https://event.inclusivday.com/registration/inscription?iw_exhibitorid=c88bcbb6-f6dd-ed11-8e89-000d3a4a8203&amp;ref=blog.lesgrandsvoisins.com\">https://event.inclusivday.com/registration/inscription?iw_exhibitorid=c88bcbb6-f6dd-ed11-8e89-000d3a4a8203&amp;ref=blog.lesgrandsvoisins.com</a></p><p data-block-key=\"dig04\"><a href=\"https://blog.lesgrandsvoisins.com/nous-a-inclusivday-cnit-10-mai-2023/\">https://blog.lesgrandsvoisins.com/nous-a-inclusivday-cnit-10-mai-2023/</a></p><p data-block-key=\"e8clf\">0  de notre objectif de 650  pour le financement de notre participation Inclusiv''Day, merci de penser  acheter nos services ou <a href=\"https://www.lesgrandsvoisins.com/dons?ref=blog.lesgrandsvoisins.com\">effectuer un don</a> s''il vous plat.</p><p data-block-key=\"9ekg2\">DATE ET HEURE</p><p data-block-key=\"8njn5\">mercredi mai 10, 202308:30 19:00 (Europe/Paris) <a href=\"https://www.lesgrandsvoisins.com/event/inclusiv-day-au-cnit-defense-paris-25/register?ref=blog.lesgrandsvoisins.com#\">Ajouter au calendrier</a></p><p data-block-key=\"1greu\">LIEU</p><p data-block-key=\"7gns1\">CNIT la Dfense2, parvis de La Dfense<br/>92800 Puteaux<br/></p>"}]', ,, 45, 'yellow-tournesol', NULL, NULL, true, true, true, NULL));
