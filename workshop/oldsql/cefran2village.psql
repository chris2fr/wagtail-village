INSERT INTO public.wagtailcore_revision VALUES (110, '2024-05-08 16:21:00.583043+00', '{"pk": 39, "body": "[{\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"plzmd\\\">Les Grands Voisins est un beau nom et un bel titre. Par plusieurs tentatives, nous avions voulu proposer d''autres noms pour faire un peu plus branch\\u00e9s, un peu plus webs. Ces noms ont \\u00e9t\\u00e9 lgvmail.com, gvois.org, lesgv.com, etc. Finalement, jusqu''\\u00e0 ce que nous d\\u00e9cidons d''un nom de domaine autre que LesGrandsVoisins.com, nous sommes LesGrandsVoisins.com.</p><p data-block-key=\\\"d6k9c\\\">Par contre, nous avons approuv\\u00e9 en Conseil des Voisins l''utilisation de ResDigita.com pour nos activit\\u00e9s num\\u00e9riques.</p><p data-block-key=\\\"1aeud\\\">C''est un rappel comme quoi, quand les choses sont d\\u00e9cid\\u00e9es, elles sont d\\u00e9cid\\u00e9es. Avant qu''elles ne soient d\\u00e9cid\\u00e9es, elles ne le sont pas.</p><p data-block-key=\\\"4m25l\\\"><a href=\\\"https://www.lesgrandsvoisins.com/?ref=blog.lesgrandsvoisins.com\\\">https://www.lesgrandsvoisins.com</a></p><p data-block-key=\\\"7u3qc\\\"><a href=\\\"https://www.resdigita.com/?ref=blog.lesgrandsvoisins.com\\\">https://www.resdigita.com</a></p>\", \"id\": \"8b180812-b30b-4bf5-b70c-cc5690fef15b\"}]", "date": "2023-04-24T10:18:00Z", "live": true, "path": "000100020003000Q", "slug": "nom-les-grands-voisins", "depth": 4, "owner": 1, "title": "Les Grands Voisins par un autre nom est toujours LesGrandsVoisins.com ?", "locale": 1, "locked": false, "authors": [1], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/nom-les-grands-voisins/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "Les Grands Voisins par un autre nom est toujours LesGrandsVoisins.com ?", "entry_tags": [], "go_live_at": null, "draft_title": "Les Grands Voisins par un autre nom est toujours LesGrandsVoisins.com ?", "content_type": 71, "header_image": 46, "header_large": true, "header_darken": true, "live_revision": null, "show_in_menus": true, "blog_categories": [], "header_cta_link": null, "header_cta_text": null, "latest_revision": null, "translation_key": "67b0da7d-4e5d-4450-be9e-4c0e8f13c079", "entry_categories": [], "header_cta_label": null, "header_with_title": true, "last_published_at": null, "first_published_at": null, "header_color_class": "pink-macaron", "search_description": "Les Grands Voisins est un beau nom et un bel titre. Par plusieurs tentatives, nous avions voulu proposer d''autres noms []", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": null}', NULL, '39', 1, 71, 1, 'Les Grands Voisins par un autre nom est toujours LesGrandsVoisins.com ?');
INSERT INTO public.wagtailcore_revision VALUES (71, '2024-05-06 03:38:15.774371+00', '{"pk": 31, "body": "[{\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"71524\\\">Pouss\\u00e9s par la Mission des Grands Voisins et le <a href=\\\"https://contractfortheweb.org/fr/?ref=blog.lesgrandsvoisins.com\\\">Contrat pour le Web</a>, nous avan\\u00e7ons avec assurance dans <b>le num\\u00e9rique interd\\u00e9pendant et autonome</b> des Grands Voisins.</p><ul><li data-block-key=\\\"1mlno\\\"><a href=\\\"https://blog.lesgrandsvoisins.com/salle-de-sociabilite-numerique/\\\">Le num\\u00e9rique cr\\u00e9atif des Grands Voisins \\u00e0 Paris le lundi de 16h \\u00e0 21h30</a></li><li data-block-key=\\\"9pddv\\\"><a href=\\\"https://homepage-dashboard.resdigita.com/\\\">homepage-dashboard.ResDigita.Com</a></li></ul><h2 data-block-key=\\\"be8ku\\\">Articles de blog sur le num\\u00e9rique</h2><p data-block-key=\\\"f0sfl\\\"><a href=\\\"https://blog.lesgrandsvoisins.com/tag/resdigita/\\\">Voici des articles de blog sur le num\\u00e9rique</a></p><h2 data-block-key=\\\"fbrjt\\\">Interd\\u00e9pendance num\\u00e9rique</h2><p data-block-key=\\\"d47rp\\\">Nous aimerions aussi dans notre IT mettre en place un principe d\\u2019\\u00e9quilibre entre l\\u2019autonomie et l\\u2019int\\u00e9gration. Nous sommes assez fiers de faire notre propre serveur de courriels. Nous aimons les logiciels libres. La mani\\u00e8re dont nous faisons est d\\u00e9j\\u00e0 ce que nous faisons.</p><p data-block-key=\\\"a80nv\\\">Avec votre compte vous avez une identit\\u00e9 centralis\\u00e9e et contr\\u00f4l\\u00e9e \\u00e0 partir de laquelle vous pouvez acc\\u00e9der \\u00e0 tout ce qui est <a href=\\\"https://lesgrandsvoisins.com/?ref=blog.lesgrandsvoisins.com\\\">lesgrandsvoisins.com:</a> dispositifs de communication, courriel dont listes de diffusion, le tout dans les serveurs autonomes pour la plupart. Nous facteurs envoient les courriels directement aux destinataires sans passer par aucun interm\\u00e9diaire autre. <a href=\\\"https://www.idgouv.com/?ref=blog.lesgrandsvoisins.com\\\">Votre compte et mot de passe</a> sont stock\\u00e9s dans un annuaire LDAP avec un acc\\u00e8s \\u00e0 authentification unique. L''association se met en posture d''\\u00eatre directement redevable envers chaque usager, et c''est tellement important dans ce monde num\\u00e9rique !</p><h2 data-block-key=\\\"63eiq\\\">Le Num\\u00e9rique Cr\\u00e9atif des Grands Voisins</h2><p data-block-key=\\\"daglo\\\">Suite \\u00e0 une rencontre de voeux organis\\u00e9e par\\u00a0<a href=\\\"https://mairie14.paris.fr/pages/maison-des-associations-11177\\\">la mairie et la MVAC du 14e arrondissement</a>, nous avons lanc\\u00e9 avec\\u00a0<a href=\\\"https://www.laressourceriecreative.com/\\\">la Ressourcerie Cr\\u00e9ative</a> <a href=\\\"https://budgetparticipatif.paris.fr/bp/jsp/site/Portal.jsp?page=idee&amp;campagne=H&amp;idee=1445\\\">un projet de budget participatif de Paris \\u00ab Le num\\u00e9rique cr\\u00e9atif \\u00bb\\u00a0</a>. Nous sollicitons le soutien de la\\u00a0<a href=\\\"https://mairie14.paris.fr/\\\">Mairie du 14e</a>\\u00a0et d\\u2019autres associations et partenaires potentiels.</p><p data-block-key=\\\"dam56\\\">Le projet de num\\u00e9rique cr\\u00e9atif est une proposition de jardinage num\\u00e9rique en deux volets: un volet serveur et un volet \\u00e9quipement de salle pouvant \\u00eatre ponctuellement transform\\u00e9 dans un lieu social informatique. L\\u2019objectif est d\\u2019encourager l\\u2019\\u00e9panouissement digital local sur la base des missions et\\u00a0<a href=\\\"https://lesgrandsvoisins.com/\\\">lesgrandsvoisins.com</a>\\u00a0et\\u00a0<a href=\\\"https://contractfortheweb.org/fr\\\">contractfortheweb.org</a>.</p><p data-block-key=\\\"fgeot\\\">Le c\\u00f4t\\u00e9 serveur est une armoire ferm\\u00e9e \\u00e0 clef avec des ressources qui soient disponibles via l\\u2019internet aux usagers \\u00e0 partir de logiciels libres.</p><p data-block-key=\\\"60ons\\\">La construction de la salle consiste en un nombre d\\u2019ordinateurs portables et d\\u2019\\u00e9quipements informatiques dans une salle qui peuvent \\u00eatre sortis et utilis\\u00e9 ponctuellement pour des besoins variables depuis le franchissement de la fracture num\\u00e9rique jusqu\\u2019au d\\u00e9veloppement vid\\u00e9o, design, programmation, 3D, administration, etc.</p><p data-block-key=\\\"2ikvn\\\">Le projet avait \\u00e9t\\u00e9 d\\u00e9pos\\u00e9 avec un partenariat avec La Ressourcerie Cr\\u00e9ative, soit point d\\u2019acc\\u00e8s num\\u00e9rique au 14i\\u00e8me. Aujourd\\u2019hui, je souhaite aussi solliciter l\\u2019int\\u00e9r\\u00eat d\\u2019autres associations telles que &amp;aprilorg, &amp;lespetitsdebrouillardsorg et &amp;antanakcom.</p><p data-block-key=\\\"5rs20\\\"><a href=\\\"https://wiki.lesgrandsvoisins.com/fr/acts/budgetpartparis\\\">D\\u00e9couvrir</a></p><ul><li data-block-key=\\\"ell5j\\\">Salle de sociabilit\\u00e9 num\\u00e9rique<ul><li data-block-key=\\\"d5n00\\\">Atelier multi-niveaux, multi-comp\\u00e9tences</li><li data-block-key=\\\"mrk8\\\">Le lundi de 16h \\u00e0 21h30 \\u00e0 l''Atelier de la Ressourcerie Cr\\u00e9ative<br/>au 93 ave du G\\u00e9n\\u00e9ral Leclerc 75014 PARIS</li></ul></li><li data-block-key=\\\"5r8p7\\\">Serveurs de sociabilit\\u00e9 num\\u00e9rique<ul><li data-block-key=\\\"at3k3\\\">Serveurs h\\u00e9berg\\u00e9s et redond\\u00e9s pour les activit\\u00e9s en lien avec la Salle</li></ul></li><li data-block-key=\\\"9muko\\\">Aide mat\\u00e9riel<ul><li data-block-key=\\\"ea75f\\\">Ce projet am\\u00e8ne du mat\\u00e9riel \\u00e0 partager entre associations<br/>(montage audio-visuel, etc.)</li><li data-block-key=\\\"3kjd8\\\">Trouver une solution pour les maux de passe, etc.</li><li data-block-key=\\\"b615g\\\">Accompagner m\\u00eame des projets pointus</li></ul></li><li data-block-key=\\\"ahj44\\\">M\\u00e9c\\u00e9nat<ul><li data-block-key=\\\"251rs\\\">M\\u00e9c\\u00e9nat de comp\\u00e9tences<br/>Nous avons besoin de gens pour organiser les activit\\u00e9s</li><li data-block-key=\\\"807k8\\\">Dons<br/>Nous mettons \\u00e0 bon usage vos contributions financier en levier du Laur\\u00e9at</li><li data-block-key=\\\"eqcs6\\\">M\\u00e9c\\u00e9nat en nature<br/>Dons d''informatique, audio-visuel, etc. Services, ... Tout sera valoris\\u00e9.</li></ul></li><li data-block-key=\\\"9prgn\\\">Accompagnement num\\u00e9rique<ul><li data-block-key=\\\"5ln8a\\\">Aider les commer\\u00e7ants et ind\\u00e9pendants, des marginalis\\u00e9s et des pros</li><li data-block-key=\\\"7pfjt\\\">Adh\\u00e9sions au Contrat pour le web dans tout ce dont nous faisons</li></ul></li></ul><h2 data-block-key=\\\"52vg8\\\">ResDigita</h2><p data-block-key=\\\"6mjam\\\">Nous avions pr\\u00e9f\\u00e9r\\u00e9 d\\u00e9nommer notre activit\\u00e9 num\\u00e9rique \\u00ab ResDigita \\u00bb lors d''un Conseil des Voisins en f\\u00e9vrier 2023. Voici nos diff\\u00e9rentes activit\\u00e9s en lien avec le num\\u00e9rique.</p><ul><li data-block-key=\\\"7rqbs\\\">Serveur Email Public<br/>Nous avons un serveur public de courriel (dite SMTP) qui livre et re\\u00e7oit \\u00e0 notre place nos courriels, que ce soit de nos bo\\u00eetes individuelles (votre domaine si vous voulez) ou de nos listes de diffusion (attention aux usages svp).</li><li data-block-key=\\\"b6reg\\\">Serveur de publication et de gestion de contenus webs</li><li data-block-key=\\\"56nm9\\\">Serveur de Webmail, de calendrier et de listes de diffusion</li><li data-block-key=\\\"1hf3d\\\">Serveurs ERP</li><li data-block-key=\\\"djpc5\\\">Serveurs de gestion de mots de passe</li></ul>\", \"id\": \"9c96dd65-5feb-4d6b-874d-cdc6bd72a5df\"}]", "live": true, "path": "000100020005", "slug": "numerique", "depth": 3, "owner": 1, "title": "Numrique", "locale": 1, "locked": false, "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/numerique/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "Le Numrique des Grands Voisins", "go_live_at": null, "draft_title": "Numrique", "content_type": 25, "header_image": 49, "header_large": true, "header_darken": false, "live_revision": null, "show_in_menus": true, "header_cta_link": "https://www.resdigita.com", "header_cta_text": "Venez vous inscrire  Res Digita, le bras informatique des Grands Voisins", "latest_revision": null, "translation_key": "d5523f08-89a9-4b36-9553-90b11a07e13c", "contentpage_tags": [], "header_cta_label": "ResDigita.com", "header_with_title": true, "last_published_at": null, "first_published_at": null, "header_color_class": "pink-macaron", "search_description": "Pousss par la Mission des Grands Voisins et le Contrat pour le Web, nous avanons avec assurance dans le numrique interdpendant et autonome des Grands Voisins.", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": null}', NULL, '31', 1, 25, 1, 'Numrique');
INSERT INTO public.wagtailcore_revision VALUES (20, '2024-05-05 16:33:59.834055+00', '{"pk": 7, "body": "[{\"type\": \"imageandtext\", \"value\": {\"image\": 10, \"image_side\": \"right\", \"image_ratio\": \"3\", \"text\": \"<p data-block-key=\\\"8vslz\\\"><i>Changeons le monde ensembles.</i></p><p data-block-key=\\\"cbep\\\">Nous proposons notre 21e Conseil des Voisins le 28 avril \\u00e0 10h dans le 14e arrondissement \\u00e0 Paris et en ligne sur <a href=\\\"https://meet.lesgrandsvoisins.com/?ref=blog.lesgrandsvoisins.com\\\">meet.lesgrandsvoisins.com</a></p><p data-block-key=\\\"7c42l\\\">Chez Papa Montparnasse Vavin (RER B Port Royal ou Metro 4 Vavin)<br/>138 Boulevard du Montparnasse, 75014 Paris</p><p data-block-key=\\\"3h4r2\\\">(<a href=\\\"https://maps.app.goo.gl/9F1w2Fam8goZ3ASx9?ref=blog.lesgrandsvoisins.com\\\">lien Maps</a>)</p><p data-block-key=\\\"aqqh1\\\">Ordre du jour :</p><ol><li data-block-key=\\\"d29qa\\\">Comptes annuels (faibles)</li><li data-block-key=\\\"9oc8p\\\">Activit\\u00e9s : arts plastiques, num\\u00e9rique, civisme</li><li data-block-key=\\\"4fef0\\\">Communication</li></ol><p data-block-key=\\\"6p2dh\\\">Nous parlerions des sujets pr\\u00e9cis en lien avec la m\\u00e9moire de Valentin FRANCY, le num\\u00e9rique cr\\u00e9atif des Grands Voisins (Budget Participatif de Paris), efforts pour rendre des h\\u00f4pitaux hybrides aussi en galeries d''art.</p><p data-block-key=\\\"ahns\\\"><a href=\\\"https://www.instagram.com/les_grands_voisins/?utm_source=ig_embed&amp;ig_rid=f6153464-ce6a-41dd-b392-5fc008623cfa\\\">https://www.instagram.com/les_grands_voisins/?utm_source=ig_embed&amp;ig_rid=f6153464-ce6a-41dd-b392-5fc008623cfa</a></p><p data-block-key=\\\"265ce\\\"></p>\", \"link\": {\"page\": null, \"external_url\": \"\", \"text\": \"\"}, \"link_label\": \"\", \"page\": null, \"link_url\": \"\"}, \"id\": \"66ff0747-1ccf-4761-8adc-4a805dd4e883\"}]", "date": "2024-04-19T08:00:00Z", "live": true, "path": "0001000200030001", "slug": "21e-con