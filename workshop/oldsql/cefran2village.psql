INSERT INTO public.sites_faciles_contentpage VALUES (108, '[{"id": "2bb8edae-aac8-4208-bb68-677a9e5e0b00", "type": "callout", "value": {"text": "Proposition de communiqu pour la Dlgation permanente de la Rpublique Islamique de Mauritanie  lUNESCO  loccaison du retour des Etats Unis par lartiste Mohamed Mal Anine Sidi Ethmane.", "title": "", "heading_tag": "h3"}}, {"id": "a1d22914-8142-4f56-8195-acceaa062969", "type": "paragraph", "value": "<p data-block-key=\"vq1lp\">Paris, le 14 juitllet 2023</p><p data-block-key=\"bc251\">Mal A.N.C.<br/>22 Rue Duranton,<br/>75015 Paris,</p><p data-block-key=\"eq7hr\">Proposition de communiqu pour la Dlgation permanente de la Rpublique Islamique de Mauritanie  lUNESCO  loccaison du retour des Etats Unis par lartiste Mohamed Mal Anine Sidi Ethmane.</p>"}, {"id": "1ff0b627-f8af-403a-86ee-7c48dceeca4d", "type": "separator", "value": {"top_margin": 3, "bottom_margin": 3}}, {"id": "a5eb0b1f-d5a6-45ff-ba4a-04b697cfc3ad", "type": "paragraph", "value": "<p data-block-key=\"vq1lp\">La Dlgation permanente de la Rpublique Islamique de Mauritanie  l''UNESCO</p><p data-block-key=\"1ib9p\"></p><embed embedtype=\"image\" format=\"fullwidth\" id=\"13\" alt=\"\"/><p data-block-key=\"ebc01\"></p><p data-block-key=\"abvac\">flicite avec gratitude les tats Unis dAmrique</p><p data-block-key=\"405oi\"></p><embed embedtype=\"image\" format=\"fullwidth\" id=\"14\" alt=\"flag-usa.pmg\"/><p data-block-key=\"6vria\">de leur retour historique  LUNESCO</p><p data-block-key=\"fan0p\"></p><embed embedtype=\"image\" format=\"fullwidth\" id=\"12\" alt=\"\"/><p data-block-key=\"8hpaf\">Nous sommes fiers et conquis par lexcellent truchement politique de bon aloi de la Directrice Gnrale de lUNESCO, Madame Audrey Azouley, qui a dignement conduit la russite de cet vnement majeur dans lhistoire de notre organisation onusienne.</p><p data-block-key=\"d2qnj\">Nous nous joignons  la solidarit inconditionnelle de tous les tats membres de LUNESCO pour accueillir ce grand retour de nos amis communs des U.S.A.</p><p data-block-key=\"55kfk\">Merci infiniment, trs cordialement.</p><p data-block-key=\"24unt\"></p>"}, {"id": "50667e55-09ac-4afe-925a-4a699179611e", "type": "separator", "value": {"top_margin": 3, "bottom_margin": 3}}, {"id": "3d6b83ce-62cb-4b15-b0ab-13ab6c9a4a5f", "type": "paragraph", "value": "<p data-block-key=\"vq1lp\">Je vous prie de considrer cette expression avec bienveillance.</p><p data-block-key=\"fqr5o\">Mal A.N.C.<br/>mael@maelanc.com  <a href=\"http://www.maelanc.com/?ref=blog.lesgrandsvoisins.com\">www.maelanc.com</a></p>"}]', ,, 12, 'red-marianne', NULL, NULL, true, true, true, NULL));
