INSERT INTO public.wagtail_localize_string VALUES (3, '6b072eac-528c-50da-84b0-a1b20bd42743', 'L''association Les Grands Voisins', 1);
INSERT INTO public.wagtail_localize_string VALUES (4, 'da2c4959-9fea-5384-b9dc-d5cf0ea01794', 'Nous, aux GV, nous uvrons pour la comprhension entre peuples qui pourraient ne pas avoir tendance  communiquer habituellement. C''est le sens mme de notre nom :
<br/> <br/> Les  comme les autres
<br/> <br/> Grands  comme les autres sont grands
<br/> <br/> Voisins  comme quoi les autres sont proches de nous, et nous sommes aussi des grands autres d''autres', 1);
INSERT INTO public.wagtail_localize_string VALUES (5, 'f535e6e5-ea96-5af2-866b-2ed8c306d1fa', '22e Conseil des Voisins du 10 au 16 juin 2024', 1);
INSERT INTO public.wagtail_localize_string VALUES (6, '0d545263-ecb1-51c7-9fa1-c49471a63800', 'Nous tiendrions le 22e Conseil des Voisins sur trois rencontres :', 1);
INSERT INTO public.wagtail_localize_string VALUES (7, '53f0d101-f9eb-5761-be7c-f4f271ad1876', 'Lundi le 10 juin de 18h  19h30', 1);
INSERT INTO public.wagtail_localize_string VALUES (8, 'd1b4719f-1a2c-593c-9f0a-eb9261f2d901', 'Jeudi le 13 juin de 19h  20h30', 1);
INSERT INTO public.wagtail_localize_string VALUES (9, '8f901b02-e493-53be-be9c-548d7c3418f3', 'Samedi le le 15 juin de 10h30  midi', 1);
