INSERT INTO public.wagtailcore_revision VALUES (23, '2024-05-05 16:59:54.335066+00', '{"pk": 9, "body": "[{\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"td4pn\\\"><b>Laur\\u00e9at du Budget Participatif de Paris 14e 2022</b></p><p data-block-key=\\\"33qtu\\\">Vous pouvez, par votre m\\u00e9c\\u00e9nat individuel ou collective, contribuer \\u00e0 rendre l\\u2019avancement informatique plus confortable, s\\u00e9cure et accueillante pour tout.e participant.e de tout \\u00e2ge et de tout niveau aux ateliers d\\u2019entraide du Num\\u00e9rique Cr\\u00e9atif des Grands Voisins. Ce m\\u00e9c\\u00e9nat peut prendre plusieurs formes</p><p data-block-key=\\\"enrbv\\\"><b>Animer des moments de socialisation</b> le lundi de 16h \\u00e0 19h<br/>Une fois que vous avez appris l\\u2019approche, vous pouvez le reproduire ailleurs. Nous avons aussi, depuis cette ann\\u00e9e, aussi du mat\\u00e9riel en serveurs et en prises et productions audio-visuels.</p><p data-block-key=\\\"fnju2\\\"><b>Pr\\u00e9parer et \\u00e0 am\\u00e9liorer le dispositif</b> par le choix et la pr\\u00e9paration de mat\\u00e9riaux \\u00e0 acqu\\u00e9rir et \\u00e0 avoir un recul sur le d\\u00e9roulement du dispositif. Coordonnez l\\u2019int\\u00e9r\\u00eat pour des populations diff\\u00e9rentes (professeurs des \\u00e9coles, professeurs, travailleurs sociaux, animateurs, etc) et pour des interventions ponctuelles pour des questions sp\\u00e9cifiques (syst\\u00e8mes, r\\u00e9seaux, s\\u00e9curit\\u00e9, etc.).</p><p data-block-key=\\\"89gi3\\\"><b>Contribuer de l\\u2019informatique et/ou de l\\u2019argent</b> de toutes natures, soit pour nos usages \\u00e0 l\\u2019atelier, soit pour l\\u2019usage de personnes en pr\\u00e9carit\\u00e9 num\\u00e9rique. Vous pouvez effectuer des dons sur www.lesgrandsvoisins.com ou sur www.laressourceriecreative.com en pr\\u00e9cisant que votre don est destin\\u00e9 pour le num\\u00e9rique cr\\u00e9atif des Grands Voisins.</p><p data-block-key=\\\"adrct\\\"><b>Prenez contact</b> avec Chris Mann par courriel au chris@lesgrandsvoisins.com ou par t\\u00e9l\\u00e9phone au 07 81 811 811</p>\", \"id\": \"4ff174ec-5f80-416b-9860-8d3f9347d3a4\"}]", "date": "2023-07-09T13:00:00Z", "live": true, "path": "0001000200030003", "slug": "lancement-du-numerique-creatif-des-grands-voisins", "depth": 4, "owner": 1, "title": "Lancement du numrique cratif des Grands Voisins", "locale": 1, "locked": false, "authors": [1], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/lancement-du-numerique-creatif-des-grands-voisins/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "Lancement du numrique cratif des Grands Voisins", "entry_tags": [{"pk": 9, "tag": 15, "content_object": 9}], "go_live_at": null, "draft_title": "Lancement du numrique cratif des Grands Voisins", "content_type": 71, "header_image": 15, "header_large": true, "header_darken": true, "live_revision": null, "show_in_menus": true, "blog_categories": [2], "header_cta_link": null, "header_cta_text": null, "latest_revision": null, "translation_key": "9ada7ef0-9318-426e-b518-34dbc7325ee7", "entry_categories": [{"pk": 4, "page": 9, "category": 2}], "header_cta_label": null, "header_with_title": true, "last_published_at": null, "first_published_at": null, "header_color_class": "yellow-tournesol", "search_description": "Vous pouvez, par votre mcnat individuel ou collective, contribuer  rendre lavancement informatique plus confortable, scure et accueillante pour tout.e participant.e de tout ge et de tout niveau aux ateliers dentraide du Numrique Cratif des Grands Voisins. Ce mcnat peut prendre plusieurs formes", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": null}', NULL, '9', 1, 71, 1, 'Lancement du numrique cratif des Grands Voisins');
INSERT INTO public.wagtailcore_revision VALUES (169, '2024-05-26 15:34:41.798235+00', '{"pk": 3, "body": "[{\"type\": \"multicolumns\", \"value\": {\"bg_image\": null, \"bg_color_class\": \"\", \"bg_color\": \"\", \"title\": \"\", \"heading_tag\": \"h2\", \"columns\": [{\"type\": \"text\", \"value\": \"<h3 data-block-key=\\\"wopaa\\\">22e Conseil des Voisins du 10 au 16 juin 2024</h3><p data-block-key=\\\"9sn6r\\\"></p><embed embedtype=\\\"image\\\" format=\\\"left\\\" id=\\\"54\\\" alt=\\\"flyer-20240506-conseildesgv22\\\"/><p data-block-key=\\\"4jn74\\\"></p><p data-block-key=\\\"f38i5\\\">Nous tiendrions le 22e Conseil des Voisins sur trois rencontres :</p><ul><li data-block-key=\\\"sqrq\\\">Lundi le 10 juin de 18h \\u00e0 19h30</li><li data-block-key=\\\"5sba\\\">Jeudi le 13 juin de 19h \\u00e0 20h30</li><li data-block-key=\\\"fabkv\\\">Samedi le le 15 juin de 10h30 \\u00e0 midi</li></ul><p data-block-key=\\\"a6q9u\\\">Ordre du jour :</p><ol><li data-block-key=\\\"43o40\\\">Num\\u00e9rique</li><li data-block-key=\\\"oioh\\\">\\u00c9ventuellement Civisme</li><li data-block-key=\\\"7l6q8\\\">Communication</li></ol>\", \"id\": \"81d80008-6286-46b4-800e-edf8d4fce080\"}, {\"type\": \"text\", \"value\": \"<h3 data-block-key=\\\"avd09\\\"><b>Permanence le lundi de midi \\u00e0 17h</b></h3><p data-block-key=\\\"e0hvf\\\"></p><embed embedtype=\\\"image\\\" format=\\\"left\\\" id=\\\"52\\\" alt=\\\"Abaker-Atelier\\\"/><p data-block-key=\\\"5c5gg\\\"></p><p data-block-key=\\\"dqj9a\\\">Venez \\u00e0 notre permanence <b>le lundi de 12h \\u00e0 17h</b><br/>en personne \\u00e0 <b>93 ave. Du G\\u00e9n. Leclerc</b> 75014 Paris (M\\u00e9tro Al\\u00e9sia, Atelier de la Ressourcerie, <i>7e boutique sur la gauche</i>) ou <a href=\\\"https://meet.lesg/\\\">meet.lesgrandsvoisins.com pour participer en hybride</a>.</p>\", \"id\": \"460c495c-476c-43f1-bb69-aff7bf1509ea\"}, {\"type\": \"text\", \"value\": \"<h3 data-block-key=\\\"avd09\\\"><a href=\\\"https://www.lesgrandsvoisins.fr/numerique/numerique-creatif/sociabilite-numerique/\\\"><b>Sociabilit\\u00e9 num\\u00e9rique le lundi de 17h \\u00e0 21h30</b></a></h3><embed embedtype=\\\"image\\\" format=\\\"right\\\" id=\\\"22\\\" alt=\\\"numasso\\\"/><p data-block-key=\\\"cda90\\\"><b>Le lundi de 17h \\u00e0 21h30</b> se tient un atelier d\\u2019entraide num\\u00e9rique \\u00e0 l\\u2019Atelier au <b>93 ave. du G\\u00e9n. Leclerc 75014 PARIS</b> \\u00e0 l\\u2019int\\u00e9rieur du <i>centre commercial 7e boutique sur la gauche</i></p>\", \"id\": \"6fc069f3-372a-447a-8d33-fd26389bdac8\"}]}, \"id\": \"c58f3abe-971d-41a7-ad49-f547da49dd04\"}, {\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"q7an3\\\"><a href=\\\"https://list.lesgrandsvoisins.com/subscription/form\\\"><b><i>Pensez \\u00e0 vous inscrire sur notre liste d''infos !</i></b></a></p><p data-block-key=\\\"evpog\\\">Nous sommes une association d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral agissant de mani\\u00e8re contr\\u00f4l\\u00e9e en subsidiarit\\u00e9 ouverte pour toute organisation et tout individu qui en fait la d\\u00e9cision pour soi-m\\u00eame principalement en ces trois domaines <a href=\\\"https://www.lesgrandsvoisins.fr/communautes/\\\">de Discussions</a>, <a href=\\\"https://www.lesgrandsvoisins.fr/arts/\\\">d''Arts plastiques</a> et <a href=\\\"https://www.lesgrandsvoisins.fr/numerique/\\\">de Num\\u00e9rique</a> selon <a href=\\\"https://www.lesgrandsvoisins.fr/mission/\\\">notre d\\u00e9claration de mission</a>.</p><p data-block-key=\\\"bj8u6\\\"><a href=\\\"https://www.lesgrandsvoisins.fr/communautes/\\\"><b>Discussions</b></a> :</p><ul><li data-block-key=\\\"61obe\\\">Nous proposons des conditions encourageant la compr\\u00e9hension entre individus et organisations de perspectives diff\\u00e9rentes.</li><li data-block-key=\\\"9oi9e\\\">Nous ouvrons notre gouvernance pour des votes en Conseils des Voisins.</li></ul><p data-block-key=\\\"65va7\\\"><a href=\\\"https://www.lesgrandsvoisins.fr/arts/\\\"><b>Arts plastiques</b></a> :</p><ul><li data-block-key=\\\"a2qag\\\">Nous encourageons l\\u2019utilisation hybrides d\\u2019espace (commerces, h\\u00f4pitaux, caf\\u00e9s, etc.) aussi en gall\\u00e9ries locales.</li><li data-block-key=\\\"2ek3r\\\">Nous accompagnons des institutions pour trouver une juste place pour les arts plastiques dans leurs locaux.</li></ul><p data-block-key=\\\"91unk\\\"><a href=\\\"https://www.lesgrandsvoisins.fr/numerique/\\\"><b>Num\\u00e9rique</b></a> :</p><ul><li data-block-key=\\\"afesu\\\">Nous maintenons une constellation de serveurs et de services internet -- tableau de bord, cr\\u00e9ation et gestion de compte, courriel, rencontres hybrides pr\\u00e9sence-distance, listes de diffusion num\\u00e9riques, blogs, serveurs webs, gestionnaire de mot de passe, enregistrement dans le nuage, forums de discussion, gestion de la connaissance, \\u00e9dition markdown, wikis, \\u00e9dition bueautique, etc. -- pour, par, et de nous-m\\u00eames et d''autres.</li><li data-block-key=\\\"ej6ca\\\">Nous mettons \\u00e0 disposition des talents et des mat\\u00e9riaux dans des salles de sociabilit\\u00e9 num\\u00e9rique et dans des syst\\u00e8mes de pr\\u00eats et \\u00e9changes de mat\\u00e9riels \\u00e0 l''attention d''individus et d''organisations de tous niveaux.</li></ul><p data-block-key=\\\"apjqo\\\">Voici quelques entit\\u00e9s Grandes Voisines qui soutiennent notre d\\u00e9marche: A Good Solution, Banya Tour, Robert Baldridge, BMCP, Breast Freedom, Le Bureau du Cr\\u00e9ateur, Le Chiffon, Les Jardins du Num\\u00e9rique, Olivier Duhec, Inno Cube, Caroline Lhomme, Francesco du 59 Rivoli, Amadou Gaye, Informatruck, Inolib, KaceKode, Ada Matus, L\\u2019Odys\\u00e9e, La Ressourcerie Cr\\u00e9ative, Henry Roy, Peter Vaeth, Waka Conseil, Weezem, et peut-\\u00eatre des v\\u00f4tres \\u2026</p><p data-block-key=\\\"at7dp\\\">Pour plus d\\u2019informations, contactez-nous\\u00a0: chris@lesgrandsvoisins.com +33 7 81 811 811</p><p data-block-key=\\\"4coel\\\"></p>\", \"id\": \"d3e4fb2e-7b28-4da0-99d3-09c2868a013f\"}, {\"type\": \"video\", \"value\": {\"title\": \"Les Grands Voisins, la cit\\u00e9 r\\u00eav\\u00e9e\", \"caption\": \"c 3, 2020 R\\u00e9alisation : Bastien Simon  Pitch : Ma\\u00ebl, artiste sans papier, Adrien, luthier musicien et d''autres r\\u00e9sidents venus de tous horizons, s''organisent pour donner naissance \\u00e0 une utopie moderne en plein coeur de Paris, un village solidaire de pr\\u00e8s de 2000 personnes : Les Grands Voisins. \\u00c0 travers leurs trajectoires et celles des membres fondateurs du lieu, le film interroge notre d\\u00e9sir et notre capacit\\u00e9 \\u00e0 inventer d\\u2019autres mani\\u00e8res de vivre ensemble. Que retiendrons-nous de cette exp\\u00e9rience collective ? Pourra-t-elle perdurer, essaimer, r\\u00e9sonner ailleurs ?\", \"url\": \"https://www.youtube.com/embed/qZqErGfNF_o?feature=oembed\"}, \"id\": \"f53195d5-0de5-479d-aef1-6151fbd266e0\"}, {\"type\": \"accordions\", \"value\": [{\"type\": \"title\", \"value\": \"Les Grands Voisins\", \"id\": \"6c6ec6eb-93af-4f4a-97e4-2f91d2498c57\"}, {\"type\": \"accordion\", \"value\": {\"title\": \"Sites Webs des Grands Voisins\", \"content\": \"<p data-block-key=\\\"h0w28\\\"><a href=\\\"https://www.lesgrandsvoisins.fr\\\">LesGrandsVoisins.fr</a> : Le site public principal des Grands Voisins</p><p data-block-key=\\\"6dm50\\\"><a href=\\\"https://www.resdigita.com\\\">ResDigita.com</a> : Le num\\u00e9rique des Grands Voisins se nomme ResDigita.</p><p data-block-key=\\\"3ntu0\\\"><a href=\\\"https://blog.lesgrandsvoisins.com\\\">Blog.LesGrandsVoisins.com</a> : Notre blog (accessible depuis le site principal)</p>\"}, \"id\": \"d2cecf58-044d-441c-849d-2d98d2085f7f\"}, {\"type\": \"accordion\", \"value\": {\"title\": \"M\\u00e9dias Sociaux des Grands Voisins\", \"content\": \"<p data-block-key=\\\"h0w28\\\"><a href=\\\"https://www.instagram.com/les_grands_voisins/\\\">Instagram@les_grands_voisins</a></p><p data-block-key=\\\"eseqg\\\">Facebook</p><p da