INSERT INTO public.sites_faciles_contentpage VALUES (112, '[{"id": "e1c2a258-883d-4473-b38b-6c715235f8fb", "type": "paragraph", "value": "<p data-block-key=\"vz7ve\">Voici les premiers mots de Thimot sur un ordinateur</p><p data-block-key=\"f50ls\">Salut mon ami comment vas-tu ? Si tout va bien, DIEU merci. Et les activits a marche bien ou pas ? Cela fait un bon moment quon ne se voit pas. Jespre quon se reverra trs prochainement.</p><p data-block-key=\"649qv\">Et Timothe revient deux semaines plus tard pour ajouter  l''article.</p><p data-block-key=\"3iblt\">Bonjour les Grands Voisins,</p><p data-block-key=\"5mrg4\">Je suis ravi dentendre parler de vous. je suis encore l aujourdhui pour me connecter au monde de linformatique, du numrique. Latelier dentraide numrique est vraiment important et la ressourcerie crative comme son nom lindique est une chose a dcouvrir tant donn que cest vraiment un endroit pour se ressourcer et crer.</p><p data-block-key=\"6nhgc\">Ne dit-on pas que lanalphabte du 21 sicle nest plus celui qui na jamais t a lcole mais cest celui qui na pas de notion en informatique.</p>"}, {"id": "5fc5066d-aa4f-4535-aca9-56b0b6ab9d2e", "type": "image", "value": {"alt": "Timothe revient et tape tout seul", "url": "", "image": 18, "title": "Timothe revient et tape tout seul", "caption": "", "heading_tag": "h3"}}]', ,, 19, 'blue-cumulus', NULL, NULL, true, true, true, NULL));
