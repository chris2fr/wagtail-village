INSERT INTO public.sites_faciles_contentpage VALUES (4, '[{"id": "3c0406e1-9c83-40fe-b9d1-5f24ad2ca71a", "type": "alert", "value": {"level": "info", "title": "Mentions lgales", "description": "Entrez ici les mentions lgales du site.<br />\n                    <a href=\"https://www.francenum.gouv.fr/guides-et-conseils/developpement-commercial/site-web/quelles-sont-les-mentions-legales-pour-un-site\">\n                    Que doivent-elles obligatoirement contenir?</a>", "heading_tag": "h2"}}, {"id": "2f1ba0f0-e45b-4023-8091-394a5657cd34", "type": "paragraph", "value": "\n                <p>Daprs la <a href=\"https://www.systeme-de-design.gouv.fr/elements-d-interface/composants/pied-de-page\">documentation du systme de design</a>,\n                le pied de page doit contenir a minima les quatre liens suivants :</p>\n                <ul>\n                    <li>Accessibilit : non/partiellement/totalement conforme</li>\n                    <li>Mentions lgales</li>\n                    <li>Donnes personnelles</li>\n                    <li>Gestion des cookies</li>\n                </ul>\n                <p>Ces deux derniers peuvent pointer vers des pages  part entire ou des sections de cette page.</p>\n                "}]', NULL, NULL, NULL, false, NULL, false, false, NULL);
